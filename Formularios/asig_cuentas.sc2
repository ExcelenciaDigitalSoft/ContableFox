		thisform.label8.caption=cuencont.nombre
		Seek thisform.txtFaccom.value
		thisform.label9.caption=cuencont.nombre
		Seek thisform.text1.value
		thisform.label12.caption=cuencont.nombre
		Seek thisform.text7.value
		thisform.label27.caption=cuencont.nombre
		Seek thisform.text6.value
		thisform.label25.caption=cuencont.nombre
		
		
		
		
		select bancos
		set orde to codigo
		seek thisform.text2.value
		thisform.label16.caption=nombre
		seek thisform.text3.value
		thisform.label21.caption=nombre
		seek thisform.text4.value
		thisform.label22.caption=nombre
		seek thisform.text5.value
		thisform.label23.caption=nombre
	ENDPROC

	PROCEDURE Text1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
			do form busqueda with 'cuencont','','Codigo','Nombre' to this.value
		endif
		
	ENDPROC

	PROCEDURE Text1.LostFocus
		if val(this.value)#0
			this.value=ceros2(this.value,6)
			select cuencont
			set order to codigo
			seek this.value
			if !found()
				=messagebox('Este Codigo no Pertenece a una Cuenta Cargada',48,'Control')			
				this.value=space(6)
				node
			else
				thisform.label12.caption=cuencont.nombre			
			endif
		else
			thisform.label12.caption=""	
		endif
	ENDPROC

	PROCEDURE Text2.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
				do form busqueda with 'bancos',allt(this.value)+chr(nkeycode),"Codigo","Nombre";
				,"Tipo_Banco","=","N","Codigo","Nombre" to this.value
		endif
	ENDPROC

	PROCEDURE Text2.LostFocus
		if val(this.value)#0
			this.value=ceros(this.value,3)
			sele bancos
			set filter to 
			set order to codigo
			seek this.value
			if found()
				If tipo_banco="N"
					thisform.label16.caption=nombre
				Else	
					messagebox("Esta es una Cuenta Bancaria",48,"Control")
					thisform.label16.caption=""
					this.value=""
					node
				EndIf	
			else
				thisform.label16.caption=''
			endif
		endif
	ENDPROC

