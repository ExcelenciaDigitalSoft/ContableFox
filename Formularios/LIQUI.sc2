	PROCEDURE txtProveedor.LostFocus
		If val(this.value)#0
			select proveedores
			Set Filter To 
			this.value=ceros(this.value,6)	
			set orde to codigo
			seek this.value
			if found()
				If sector="002"
					thisform.TxtNompro.Value=proveedores.nombre
					select liquidaciones1
					set orde to numero
					seek thisform.txtnumero.Value
					If !Found()
						thisform.carga_pendientes()
					Endif	
				Else
					this.Value=""
					thisform.TxtNompro.Value=""
					node
				Endif	
			Else
				thisform.TxtNompro.Value=""
				messagebox("Código de Proveedor Incorrecto",48,"Control")
				this.value=space(3)
				node
			Endif
		else
			if lastkey()=13 or lastkey()=24 or lastkey()=9
				thisform.TxtNompro.Value=""
				if Messagebox("Desea Cargar Liquidación sin Código de Proveedor",36,"Control")=7
					node
				EndIf	
			Endif	
		EndIf
		thisform.label100.visible=.f.
		
	ENDPROC

