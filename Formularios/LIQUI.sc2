	PROCEDURE Xpbutton2.Click
		If (thisform.asiento1.text5.value+thisform.asiento1.text6.value)=0
			messagebox("Debe Cargar Asiento Contable",48,"Control")
			thisform.asiento1.grid1.column1.text1.setfocus
			return
		EndIf	
		If Thisform.asiento1.text4.value#0
			messagebox("No se puede Grabar un Asiento con Diferencia."+chr(13)+"Intente Generar un Asiento Automático.",48,"Control")
			thisform.asiento1.grid1.column1.text1.setfocus
			return	
		EndIf
		if messagebox('Confirma Grabar Datos',36,'Grabar')=6
			******* Grabo Liquidaciones *******
			select liquidaciones
			=tableupdate(.t.)
			******* Grabo en Det_Fac *******
			Select aux_detfacliq
			Go top
			DO while not Eof() and Val(periodo)#0
				Select det_fac
				Go record aux_detfacliq.registro
				Replace debito with aux_detfacliq.debito
				Replace ajuste with aux_detfacliq.ajuste
				Replace importe_liq with aux_detfacliq.importe_li
				Replace num_liq with thisform.txtnumero.value
				Select aux_detfacliq
				skip
			Enddo
			Select aux_detfacliq
			=TableRevert(.t.)
			thisform.grdAux_det.Refresh
			******* Graba Factura Venta*******
			thisform.graba_factura_venta("G") &&Graba facven VER!!!!
			*******	Grabo Asiento Contable *******
			If (thisform.asiento1.text5.value+thisform.asiento1.text6.value)#0
				thisform.asiento(thisform.txtnumero.value,'grabar')
			EndIf		
			******* Grabo Mayores de Tesoreria *******
			select movimientos_bancos
			set orde to clav_liqui
			seek thisform.txtnumero.value+thisform.codigo1.value
			if !found()
				append blank
			endif	
			repl num_liqui with thisform.txtnumero.value
			repl codigo_banco with thisform.codigo1.value
			repl det_codban with thisform.text4.value
			repl tipo_banco with "N"
			repl fecha_emision with thisform.txtfecha.value
			repl tipo_movimiento with '006'
			repl debe_haber with 'D'
			repl detalle with "Liquidación N°: "+thisform.txtNumero.value+" "+Alltr(thisform.label6.caption)+" "+alltr(thisform.txtnompro.value)
			repl importe with thisform.txttot_liqui.value 	
			******* Grabo Efectivo en Mayores de Tesoreria *******
			if thisform.text2.value#0
				store space(0) to efectivo_teso,efectivo_nombre
				Select asig_cuentas
				Go top
				efectivo_teso=asig_cuentas.caja_teso
				efectivo_nombre=vercod("bancos",efectivo_teso)
				select movimientos_bancos
				set orde to clav_liqui
				seek thisform.txtnumero.value+efectivo_teso
				if !found()
					append blank
				endif	
				repl num_liqui with thisform.txtnumero.value
				repl codigo_banco with efectivo_teso
				repl det_codban with efectivo_nombre
				repl tipo_banco with "N"
				repl fecha_emision with thisform.txtfecha.value
				repl tipo_movimiento with '006'
				repl debe_haber with 'D'
				repl detalle with "Liquidación N°: "+thisform.txtNumero.value+" "+Alltr(thisform.label6.caption)+" "+alltr(thisform.txtnompro.value)
				repl importe with thisform.text2.value 		
			EndIf	
			******* Grabo Retenciones Obtenidas *******
			select aux_retenciones
			go top
			Do while not eof() and val(Codigo)#0
				scatter memvar
				m.num_liqui=thisform.txtNumero.value
				m.Fec_Ing=thisform.text3.value
				m.proveedor_ingreso=thisform.txtProveedor.value
				m.nompro_ingreso=thisform.txtNomPro.value
				m.det_num_liqui=thisform.txtNum_liqui.value
				select retenciones
				if m.registro=0
					if m.importe#0
						append blank
						gather memvar
					endif
				else
					go record m.registro
					if m.importe=0
						dele
					else
						gather memvar	
					endif
				endif
				select aux_retenciones
				Skip
			EndDo
			******* Grabo Cheques de Terceros *******
			select aux_terceros
			go top
			scan
				scatter memvar
				m.num_liqui=thisform.txtNumero.value
				select chequeter
				if m.regi=0
					if m.importe#0
						append blank
						gather memvar
					endif	
				else
					go record m.regi
					if m.importe=0
						dele 
					else
						gather memvar	
					endif	
				endif
				select aux_terceros
			endscan
			thisform.chequeter1.grid1.refresh
			thisform.chequeter1.visible=.f.
			thisform.chequeter1.grid1.visible=.f.	
			******* Grabo depositos *******
			select deposito2
			go top
			do while not eof() and val(codigo_ban)#0
				scatter memvar
				m.num_liqui=thisform.txtNumero.value
				m.codigo_banco=m.codigo_ban
				m.numero_comprobante=m.numero_com
				fecha_emision=fecha_emis
				fecha_vencimiento=fecha_emis
				m.tipo_movimiento='002'
				m.debe_haber='D'
				m.tipo_banco="S"
				m.det_codban=vercod("Bancos",m.codigo_banco)
				select movimientos_bancos
				if m.registro=0
					append blank
					gather memvar			
				else
					go record m.registro
					if m.importe#0
						gather memvar				
					else
						dele	
					endif	
				endif	
				select deposito2
				skip
			enddo	
			thisform.depositos1.visible=.f.
			thisform.depositos1.Grid1.visible=.f.
			******* Imprimo Informe *******
			thisform.imprimir()
			******* Inicializo el Formulario *******
			select deposito2
			=TableRevert(.t.)
			select aux_retenciones
			=tablerevert(.t.)
			select aux_terceros
			=tablerevert(.t.)
			thisform.asiento("","")
			select tabla15
			=tablerevert(.t.)
			append blank
			thisform.asiento1.grid1.refresh
			thisform.Label6.Caption=""
			Select liquidaciones
			Count to a for !dele()
			If a #0
				set orde to numero
				go bott
				thisform.ultimo=Val(numero)
				thisform.txtnumero.value=ceros(Str(Val(numero)+1,6),6)
				Skip
			Else
				thisform.ultimo=" "
				thisform.txtnumero.value=ceros(Str(Val("1")+1,6),6)
			endif
			thisForm.Refresh
			thisform.txtnumero.setfocus
		endif
	ENDPROC

	PROCEDURE Xpbutton3.Click
		if messagebox('Confirma Eliminar Datos',36,'Grabar')=6
			******* Borro Liquidaciones *******
			Select liquidaciones
			dele
			=tableupdate(.t.)
