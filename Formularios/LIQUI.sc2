	PROCEDURE txtNumero.LostFocus
		If Val(this.value)#0
			this.value=ceros(this.value,6)
			select liquidaciones
			set orde to numero
			seek this.value
			if !found()
				thisform.Xpbutton2.visible=.T.
				if !(val(this.value)=thisform.ultimo+1)
					messagebox("No se Puede Perder la Correlatividad de las Liquidaciones";
					+chr(13)+"El Próximo a Cargar es : "+Ceros(Str(thisform.ultimo+1,6),6),48,"Control")
					this.value=Ceros(Str(thisform.ultimo+1,6),6)
					node
				else	
					append blank
					repl numero with this.value,fecha with date()
				endif	
			else	
				&& Nº de Liquidación Encontrada
				thisform.Xpbutton2.visible=.F.
				******* Consulta Detalle de Liquidacion *******
				Select det_Fac
				Set Order To num_liq
				Seek this.Value
				DO while not Eof() and num_liq=this.Value
					Scatter memvar
					m.registro=Recno()		
					m.importe_li=m.importe_liq
					Select aux_detfacliq
					Append Blank
					Gather memvar
					Select det_fac
					skip
				EndDo		
				******* Muestro Asientos Cargados *******
				thisform.asiento(this.value,'mostrar')
				******* Cargo Retenciones *******
				select retenciones
				set orde to num_liqui
				seek this.value
				do while not eof() and num_liqui=this.value
					scatter memvar
					m.registro=recno()
					select aux_retenciones
					append blank
					gather memvar
					select retenciones
					skip
				enddo
				******* Cargo Cheques de Terceros *******
				select chequeter
				set order to num_liqui
				seek this.value
				do while not eof() and num_liqui=this.value
					scatter memvar
					m.copiado=1
					m.regi=recno()
					select aux_terceros
					append blank
					gather memvar
					select chequeter
					skip
				enddo		
				******* Carga el detalle de los depositos *******
				select movimientos_bancos
				set order to num_liqui
				seek this.value
				if found()
					do while not eof() and num_liqui=this.value
						if tipo_banco="S"
							m.registro=recno()
							scatter memvar
							m.codigo_ban=m.codigo_banco
							m.numero_com=m.numero_comprobante
							m.fecha_emis=m.fecha_emision
							select deposito2
							append blank
							gather memvar
						EndIf	
						select movimientos_bancos
						skip
					EndDo
				endif&& found
				thisform.depositos1.grid1.refresh				
			EndIf
			select aux_retenciones
			If recc()=0
				Append Blank
			EndIf
			Thisform.Grid1.refresh	
			*Select aux_detfacliq
			*If Reccount()=0
			*	Append Blank
			*Endif
			*thisform.grdAux_det.refresh
		else
			if lastkey()#27
				messagebox("Debe Carger Nº de Liquidación";
				+chr(13)+"El Próximo a Cargar es : "+Ceros(Str(thisform.ultimo+1,6),6),48,"Control")
				this.value=Ceros(Str(thisform.ultimo+1,6),6)
				node
			endif	
		EndIf
		thisform.refresh
	ENDPROC

