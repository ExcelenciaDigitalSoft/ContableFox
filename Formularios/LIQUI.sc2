	PROCEDURE graba_factura_compra
		parameters _accion &&_accion: G = Graba ; E= Elimina
		if _accion="G"
			select faccom
			set orde to num
			seek "LiquiNº:"+thisform.txtNumero.value
			if !found()
				append Blank
			endif
			repl numero with "LiquiNº:"+thisform.txtNumero.value
			repl fecha with thisform.txtFecha.value
			repl periodo with thisform.txtPer_iva.value
			repl proveedor with thisform.txtProveedor.value
			repl nombre with thisform.txtNompro.value
			repl exento with 0.00
			repl neto with thisform.txtComision.value
			repl iva with thisform.txtiva.value
			repl total with (thisform.txtComision.value+thisform.txtiva.value)
			repl gravado with (thisform.txtComision.value+thisform.txtiva.value)
			If Val(thisform.txtProveedor.value)#0
				repl cuit with verfield("Proveedores",thisform.txtProveedor.value,"cuit","Codigo")
				repl condicion with verfield("Proveedores",thisform.txtProveedor.value,"condicion","Codigo")
			EndIf	
		else
			select faccom
			set orde to num
			seek "LiquiNº:"+thisform.txtNumero.value
			if found()
				dele
			endif
		Endif	
		
	ENDPROC

	PROCEDURE graba_factura_venta
