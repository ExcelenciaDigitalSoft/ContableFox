	PROCEDURE imprimir
		if messagebox('Confirma imprimir Liquidación',36,'Imprimir')=6
			select imp_orden
			=tablerevert(.t.)
			******* Cargo Retenciones *******
			select Aux_Retenciones
			go top
			do while not eof() and Val(codigo)#0
				select imp_orden
				append blank
				repl detalle with Aux_Retenciones.codigo+" "+Alltr(Aux_Retenciones.Nombre)+" Nº: "+alltr(Aux_Retenciones.numero)
				repl fecha with {}
				repl tipo with 1
				repl importe with Aux_Retenciones.importe
				select Aux_Retenciones
				skip
			enddo	
			******* Cargo Depositos *******
			select deposito2
			go top
			do while not eof() and Val(codigo_ban)#0
				select imp_orden
				append blank
				repl detalle with "Depósito : "+Alltr(vercod("Bancos",deposito2.codigo_ban))+" Nº: "+alltr(deposito2.numero_com)
				repl fecha with {}
				repl tipo with 2
				repl importe with deposito2.importe
				select deposito2
				skip
			enddo
			******* Cheques de Terceros que Ingresaron a la Cartera *******
			select aux_terceros
			go top
			scan
				if importe#0
					select imp_orden
					append blank
					repl detalle with "N° Cheque : "+aux_terceros.comproban+" Banco : "+aux_terceros.banco
					repl fecha with aux_terceros.fechaven
					repl tipo with 2
					repl importe with aux_terceros.importe
				EndIf	
				select aux_terceros
			endscan		
			******* Cargo el Efectivo *******
			if thisform.text2.value#0
				select imp_orden
				append blank
				repl detalle with "Efectivo"
				repl fecha with {}
				repl tipo with 2
				repl importe with thisform.text2.value
			endif
			******* Ordena la Tabla Por Tipo e Imprimo el Informe *******
			select imp_orden
			set order to tipo
			Report form liquidaciones.frx NoConsole Prev	
		endif	
		
	ENDPROC

