	PROCEDURE asiento
		param campo,_tipo &&//El campo es la clave que hace referencia a los asientos.Los tipos posibles son : (sin datos es para limpiar),mostrar,grabar,eliminar
		store space(6) to asiento,cam
		cam=ceros(str(val(campo),6),6)
		with thisform
			select detacont
			set filte to periodo=periodo_actual.periodo
			select movicont 
			set filte to periodo=periodo_actual.periodo
			if empty(cam) and empty(_tipo)
				.asiento1.text4.value=0.00	
				.asiento1.text5.value=0.00	
				.asiento1.text6.value=0.00	
				.asiento1.nro.caption=""
				select tabla15
				=tablerevert(.t.)
				append blank
				.asiento1.refresh
				.asiento1.Grid1.refresh
			else	
				.asiento1.text4.value=0.00	
				.asiento1.text5.value=0.00	
				.asiento1.text6.value=0.00	
				do case
					case alltr(_tipo)='mostrar'	
						select tabla15
						=tablerevert(.t.)			
						select movicont
						set order to num_liqui
						seek cam
						if found()
							thisform.asiento1.nro.caption="Asiento N°: "+alltr(movicont.nroasiento)
							do while not eof() and num_liqui=cam
								scatte memvar
								m.registro=recno()
								sele cuencont
								set order to codigo
								seek m.codigo
								m.detalle=nombre
								sele tabla15
								appen blan
								if m.debehaber='D'
									m.debe=importe
									m.haber=0
									thisform.asiento1.text4.value=thisform.asiento1.text4.value+importe
									thisform.asiento1.text5.value=thisform.asiento1.text5.value+importe
								else
									m.haber=importe
									m.debe=0
									thisform.asiento1.text4.value=thisform.asiento1.text4.value-importe 
									thisform.asiento1.text6.value=thisform.asiento1.text6.value+importe
								endif
								gather memvar
								sele movicont
								skip
							enddo
						endif
					case alltr(_tipo)='grabar'	
						select detacont
						set order to num_liqui
						seek cam
						if found()
							asiento=nroasiento
							uni=unico
						else
							set order to nroasiento
							go bott
							asiento=ceros(str(val(nroasiento)+1,6),6)	
							uni=sys(2015)
							append blank
						endif
						repl nroasiento with asiento
						repl periodo with periodo_actual.periodo
						repl unico with uni
						repl detalle with 'Asiento de Liquidacion ('+Alltr(thisform.label6.caption)+")"
						repl leyenda with 'Liquidación N°: '+cam+' '+thisform.txtNompro.value
						repl fecha    with thisform.txtFecha.value					
						repl num_liqui with cam
						repl tipo with '2'
						sele tabla15
						go top
						do while not eof() and val(codigo)#0
							scatter memvar
							letra=iif(m.debe#0,'D','H')
							m.nroasiento=asiento
							m.num_liqui=cam
							m.codigo=m.codigo
							m.fecha=thisform.txtFecha.value
							m.unico=uni
							m.periodo=periodo_actual.periodo
							m.tipo='2'
							if letra='D'
								m.debehaber='D'
								m.importe=m.debe
							else
								m.debehaber='H'
								m.importe=m.haber
							endif	
							sele movicont
							if m.registro=0
								appe blan
								gather memvar
							else
								go record m.registro
								if m.debe=0	and m.haber=0
									dele 
								else
									gather memvar	
								endif
							endif
							sele tabla15
							skip
						enddo
					case alltr(_tipo)='eliminar'	
						select detacont
						set order to num_liqui
						seek cam
						if found()
							dele
						endif
						select movicont
						set order to num_liqui
						seek cam
						if found()
							do while not eof() and num_liqui=cam
								dele
								seek cam
							enddo
						endif					
				endcase
				.asiento1.grid1.refresh	
			endif	
			select detacont
			set filte to 
			select movicont 
			set filte to 
		endwith	
	ENDPROC

	PROCEDURE asi_automatico
		Select asig_cuentas
		Go top
		c_caja=asig_cuentas.caja
		c_ChequeTer=asig_cuentas.Chequeter
		c_comisiones_liqui=asig_cuentas.comisiones_liqui
		c_iva=asig_cuentas.ivacred
		Select tabla15
		=tablerevert(.t.)
		******* Cheques de Terceros *******
		If thisform.text5.value#0
			Appen Blank
			repl codigo with c_ChequeTer
			repl detalle with vercod("CuenCont",c_ChequeTer,"Codigo")
			repl debe with thisform.text5.value
			repl haber with 0.00
		EndIf	
		******* Efectivo *******
		If thisform.text2.value#0
			Appen Blank
			repl codigo with c_caja
			repl detalle with vercod("CuenCont",c_caja,"Codigo")
			repl debe with thisform.text2.value
			repl haber with 0.00
		EndIf
		******* Depositos *******
		if thisform.text1.value#0
			select deposito2
			go top
			do while not eof() and val(codigo_ban)#0
				select bancos
				set order to codigo
				seek deposito2.codigo_ban
				if found()
					select cuencont
					set order to codigo
					seek bancos.cuenta
					if found()
						select tabla15
						append blank
						repl codigo with bancos.cuenta,detalle with cuencont.nombre
						repl debe with deposito2.importe,haber with 0.00
					endif
				endif
				select deposito2
				skip
			enddo
		endif
		******* Comisiones de Liquidación *******
		If (thisform.txtComision.value+thisform.txtOtros_des.value)#0
			Select tabla15
			Appen Blank
			repl codigo with c_comisiones_liqui
			repl detalle with vercod("CuenCont",c_comisiones_liqui,"Codigo")
			repl debe with (thisform.txtComision.value+thisform.txtOtros_des.value),haber with 0.00
		EndIf	
		******* IVA ******* 
		if thisform.txtiva.value#0
			select cuencont
			set order to codigo
			seek c_iva
			if found()
				select tabla15
				set order to codigo
				seek c_iva
				if !found()
					append blank
				endif
				repl codigo with c_iva,detalle with cuencont.nombre
				repl debe with thisform.txtiva.value,haber with 0.00		
			endif	
		endif
		******* Retenciones *******
		If thisform.txtRetencion.value # 0
			select aux_retenciones
			go top
			scan
				select tipo_retenciones
				set orde to codigo
				seek aux_retenciones.codigo
				if found()
					select cuencont
					set order to codigo
					seek tipo_retenciones.cuenta
					if found()
						select tabla15
						append blank
						repl codigo with tipo_retenciones.cuenta,detalle with cuencont.nombre
						repl debe with aux_retenciones.importe,haber with 0.00
					EndIf	
				endif
				select aux_retenciones
			endscan	
		EndIf
		******* Cierre del Asiento Dependiendo de la Cienta de Tesoreria
		if Val(thisform.codigo1.value)#0 
			select bancos
			set order to codigo
			seek alltr(thisform.codigo1.value)
			if found()
				select cuencont
				set order to codigo
				seek bancos.cuenta
				if found()
					select tabla15
					set order to codigo
					seek bancos.cuenta
					if !found()
						appen blank
					endif	
					repl codigo with bancos.cuenta,detalle with cuencont.nombre
					repl debe with 0.00,haber with thisform.txttotal.value
				endif
			endif		
		Endif		
		select tabla15
		set orde to 0
		
	ENDPROC

	PROCEDURE calcula_tot
		With Thisform
			.txtTot.value=.txtGravado.value+.txtExento.value
			.txtIva.value=.txtGravado.value-.txtNeto.value
		EndWith
	ENDPROC

	PROCEDURE calculo
		Store 0 to thisform.totimporte.value
		Store 0 to thisform.totdebito.value
		Store 0 to thisform.totajuste.value
		Store 0 to thisform.totliqui.value
		Select aux_detfacliq
		_r=Recno()
		Go top
		DO while !Eof()
			Replace importe_li with importe-(debito+ajuste)
			thisform.totimporte.value=thisform.totimporte.value+importe
			thisform.totdebito.value=thisform.totdebito.value+debito
			thisform.totajuste.value=thisform.totajuste.value+ajuste
			thisform.totliqui.value=thisform.totliqui.value+importe_li
			skip
		EndDo
		thisform.txtTotal.Value=thisform.totliqui.value
		Go record _r
		
		
	ENDPROC

	PROCEDURE carga_pendientes
		If MessageBox("Desea ver las Obras Sociales Pendientes de Liquidación del Centro de Facturación: ";
		+Alltrim(thisform.txtNompro.Value)+"?",36,"Control")=6
			Select aux_detfac
			=TableRevert(.t.)
			Select * , verfield("Facturacion",Numero,"Cuenta","Numero") as cuenta ,verfield("Facturacion",Numero,"NomCuenta","Numero") as Nomcuenta ;
			from det_fac where centro_fac=Alltrim(thisform.txtProveedor.value);
			and num_liq=" " order by nombre;
			into cursor pendientes
			Store 0 to thisform.texttot.value
			Select pendientes
			If Reccount()#0
				Go top
