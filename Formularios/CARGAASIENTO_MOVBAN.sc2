	PROCEDURE NROASIENTO1.LostFocus
		THISFORM.grabado=.f.
		thisform.textorden.value=''
		
		
		thisform.txttipo.Value=''
		thisform.txtfechaemision.Value={}
		thisform.txtdetalle.Value=''
		thisform.txtimporte.Value=0
		thisform.txtnroregistro.Value=0
		
		
		if val(this.value)#0
			select detacont
			set filte to periodo=_periodosele  && periodo_actual.periodo
			select movicont 
			set filte to periodo=_periodosele   && periodo_actual.periodo
			
			sele tabla15
			=tablerevert(.t.)
			this.value=ceros(this.value,6)
			thisform.text4.value=0.00
			thisform.text5.value=0.00
			thisform.text6.value=0.00
			if val(this.value)<=val(thisform.text2.value)+1
				sele detacont
				set order to nroasiento
				seek this.value
				if found()
					THISFORM.grabado=.t.
					do case
						Case Val(tipo_asi)=1
							thisform.o.o3.value=0
							thisform.o.o2.value=1
							thisform.o.o1.value=0				
						Case Val(tipo_asi)=1
							thisform.o.o3.value=1
							thisform.o.o2.value=0
							thisform.o.o1.value=0				
						other
							thisform.o.o3.value=0
							thisform.o.o2.value=0
							thisform.o.o1.value=1				
					EndCase	
					thisform.Label_modelo.visible=.f.
					thisform.text3.visible=.f.
					thisform.text1.value=detalle	
					thisform.txtleyenda.value=allt(leyenda)
					thisform.fecha1.value=fecha
					sele movicont
					set order to nroasiento
					seek this.value
					if found()
						do while not eof() and nroasiento = this.value
							scatte memvar
							m.registro=recno()
							sele cuencont
							set order to codigo
							seek m.codigo
							m.detalle=nombre
							sele tabla15
							appen blan
							if m.debehaber='D'
								m.debe=importe
								m.haber=0
								thisform.text4.value=thisform.text4.value+importe
								thisform.text5.value=thisform.text5.value+importe
							else
								m.haber=importe
								m.debe=0
								thisform.text4.value=thisform.text4.value-importe 
								thisform.text6.value=thisform.text6.value+importe
							endif
							gather memvar
							sele movicont
							skip
						enddo
					EndIf
					*------ Busco si tiene movimientos contables para Eliminar. --------------------
					_nroasi=_periodosele+Alltrim(thisform.nroASIENTO1.Value)
					Select *,Recno() as registro from movimientos_bancos where asiconci=_nroasi into cursor datos
					Select datos
					If Reccount()>0
						Go top
						If tipo_movimiento='003'
							thisform.txttipo.Value='Crédito'
						EndIf
						If tipo_movimiento='004'
							thisform.txttipo.Value='Débito'
						endif
						thisform.txtfechaemision.Value=fecha_emision
						thisform.txtdetalle.Value=detalle
						thisform.txtimporte.Value=importe
						thisform.txtnroregistro.Value=registro
					endif
					
					*-------------------------------------------------------------------------------
					
					******* Muestro los datos de la auditoria *******
					Select auditoria_asientos
					Set Order To perasiento
					Seek _periodosele+thisform.nroASIENTO1.Value
					If Found()			
						thisform.text7.Value = obs
						DO case
							Case estado = 1
								thisform.estado.o1.Value = 1
								thisform.estado.o2.Value = 0
								thisform.estado.o3.Value = 0
							Case estado = 2
								thisform.estado.o2.Value = 1
								thisform.estado.o1.Value = 0
								thisform.estado.o3.Value = 0						
							Case estado = 3 
								thisform.estado.o1.Value = 0
								thisform.estado.o2.Value = 0					
								thisform.estado.o3.Value = 1
						endcase
					Else
						thisform.estado.o1.Value = 1
						thisform.estado.o2.Value = 0
						thisform.estado.o3.Value = 0				
						thisform.text7.Value = ""	
					EndiF
					*************************************************
				else
					if val(this.value)=1
						thisform.o.o3.value=0
						thisform.o.o2.value=1
						thisform.o.o1.value=0
					else
						thisform.o.o3.value=0
						thisform.o.o2.value=0
						thisform.o.o1.value=1
					endif
					thisform.Label_modelo.visible=.t.
					thisform.text3.visible=.t.
					select tabla15
					append blank
					thisform.text1.value=''
					thisform.text3.value=''
					thisform.txtleyenda.value=''
					thisform.text4.value=0.00
					thisform.text5.value=0.00
					thisform.text6.value=0.00
					thisform.estado.o1.Value = 1
					thisform.estado.o2.Value = 0
					thisform.estado.o3.Value = 0				
					thisform.text7.Value = ""				
				endif	
				sele tabla15
				go top
				
				thisform.grid1.refresh	
			else
				messagebox('Los N° de Asientos son Correlativos el Proximo a Cargar es el N°:'+ceros(str(val(thisform.text2.value)+1,6),6),48,'Control')
				this.value=ceros(str(val(thisform.text2.value)+1,6),6)
				thisform.text1.value=''
				thisform.text3.value=''
				thisform.txtleyenda.value=''
				thisform.text4.value=0.00
				thisform.text5.value=0.00
				thisform.text6.value=0.00
				thisform.estado.o1.Value = 1
				thisform.estado.o2.Value = 0
				thisform.estado.o3.Value = 0				
				thisform.text7.Value = ""			
			endif	
		else
			if lastkey()=13 or 	lastkey()=24 or lastkey()=9 or lastkey()=32
				messagebox('Debe Cargar el N° de Asiento ',48,'Control')
				node
			endif
		endif
		select detacont
		set filte to 
		select movicont 
		set filte to 
		thisform.refresh
	ENDPROC

