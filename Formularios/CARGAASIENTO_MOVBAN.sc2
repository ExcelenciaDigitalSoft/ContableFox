	PROCEDURE Xpbutton5.Click
		if thisform.text4.value#0
			if messagebox('Saldo Distinto de Cero.'+chr(13)+'¿Desea Continuar ?',36,'Error')=7
				thisform.grid1.column1.text1.setfocus
				return
			endif
		endif
		If (thisform.text5.value+thisform.text6.value)=0
			messagebox("Debe Cargar Asiento Contable",48,"Control")
			thisform.grid1.column1.text1.setfocus
			return
		EndIf	
		if val(thisform.nroasiento1.value)=0
			=messagebox('No Se Puede Grabar con N° de Asiento 0',48,'Control')
		else
			if 	messagebox('Confirma Grabar Auditoria Contable',36,'Grabar ')=6
				DO case
					Case thisform.estado.o1.Value = 1
						_estado = 1
					Case thisform.estado.o2.Value = 1
						_estado = 2
					Case thisform.estado.o3.Value = 1
						_estado = 3								
				endcase
				set filte to periodo=_periodosele  &&periodo_actual.periodo
				Select auditoria_asientos
				Set Order To perasiento
				Seek _periodosele+thisform.nroASIENTO1.Value
				If !Found()
					Append Blank
					Replace periodo with _periodosele && periodo_actual.periodo
					Replace nroasiento with thisform.nroASIENTO1.Value
					Replace fecha with Date()
					Replace estado with _estado
					Replace obs with thisform.text7.Value
					Replace op with cod_op
					Replace fecModi with Date()
				Else
					If _estado= 1
						Select auditoria_asientos
						Delete 
					else	
						Replace estado with _estado
						Replace obs with thisform.text7.Value
						Replace op with cod_op
						Replace fecModi with Date()				
					EndIf	
				Endif
			endif	
		endif	
	ENDPROC

	PROCEDURE Xpbutton6.Click
		*if thisform.text4.value#0
		*	if messagebox('Saldo Distinto de Cero.'+chr(13)+'¿Desea Continuar ?',36,'Error')=7
		**		thisform.grid1.column1.text1.setfocus
		*		return
		*	endif
		*endif
		
		
		
		If (thisform.text5.value+thisform.text6.value)=0
			messagebox("Debe Cargar Asiento Contable",48,"Control")
			thisform.grid1.column1.text1.setfocus
			return
		EndIf	
		
		If thisform.text5.value<>thisform.text6.value
			messagebox("El Debe debe ser igual al Haber",48,"Control")
			thisform.grid1.column1.text1.setfocus
			return
		EndIf	
		
		
		If thisform.grabado=.t.
		*	messagebox("El Asiento ya fue Grabado",48,"Control")
		*	return
		EndIf	
		
		if .t.  && val(thisform.nroasiento1.value)=0
		*	=messagebox('No Se Puede Grabar con N° de Asiento 0',48,'Control')
		*else
			if 	messagebox('Confirma Grabar Asiento',36,'Grabar ')=6
			
			
				sele detacont
				set filter to periodo=_periodosele   &&periodo_actual.periodo
				set order to nroasiento
				go bottom
				
				if !thisform.grabado
					_ULTIMO=NROASIENTO
					IF Ceros(Str(Val(nroasiento)+1,6),6) <>thisform.nroasiento1.value
						thisform.text2.value=nroasiento
						thisform.nroasiento1.value=Ceros(Str(Val(nroasiento)+1,6),6)
						MESSAGEBOX("Se verificó el Número del Ultimo Asiento, se utilizará: "+allt(thisform.nroasiento1.value),48,"Control")
					ENDIF
				endif
				
				
				uni=sys(2015)
				
				select detacont
				set filte to periodo=_periodosele   &&periodo_actual.periodo
		
		
				select movicont 
				set filte to periodo=_periodosele   && periodo_actual.periodo
				
				sele movicont
				set order to nrocod
				sele tabla15
				go top
				do while not eof() and val(codigo)#0
					scatter memvar
					letra=iif(m.debe#0,'D','H')
					m.nroasiento=thisform.nroasiento1.value
					m.codigo=m.codigo
					m.fecha=thisform.fecha1.value
					m.periodo=_periodosele   &&periodo_actual.periodo
					if letra='D'
						m.debehaber='D'
						m.importe=m.debe
					else
						m.debehaber='H'
						m.importe=m.haber
					endif	
					sele movicont
					if m.registro=0
						appe blan
						m.unico=uni
						gather memvar
					else
						go record m.registro
						if m.debe=0	and m.haber=0
							dele 
						else
							gather memvar	
						endif
					endif
					sele tabla15
					skip
				enddo
				=tablerevert(.t.)
				sele detacont
				seek thisform.nroasiento1.value
				if !found()
					appe blan
					repl unico with uni
				endif
				repl nroasiento with thisform.nroasiento1.value
				repl periodo with _periodosele   &&periodo_actual.periodo
				repl detalle with thisform.text1.value
				repl leyenda with allt(thisform.txtleyenda.value)
				repl fecha    with thisform.fecha1.value
				do case
					case thisform.o.o1.value=1
						repl tipo_asi with space(3)
					case thisform.o.o2.value=1
						repl tipo_asi with "001"
					case thisform.o.o3.value=1
						repl tipo_asi with "002"
				endcase
				thisform.nroasiento1.value = space(6)
				thisform.text1.value       = space(50)
				thisform.fecha1.value=date()
				thisform.nroasiento1.value=space(6)
				thisform.text1.value=space(50)
				thisform.text2.value=space(6)
				thisform.text3.value=space(3)
				thisform.text4.value=0.00
				thisform.text5.value=0.00
				thisform.text6.value=0.00
				select detacont
				set filte to 
				select movicont 
				set filte to 
				sele tabla15
				=tablerevert(.t.)
				sele detacont
				go bottom
				thisform.text2.value= nroasiento
				thisform.nroasiento1.setfocus		
			endif	
		endif	
	ENDPROC

ENDDEFINE
