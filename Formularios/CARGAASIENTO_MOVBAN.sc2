	PROCEDURE ceros
		parameter x,y
		x=allt(x)
		x=repl('0',y-len(x))+x
		return x
	ENDPROC

	PROCEDURE ceros2
		parameter x
		return allt(x)+repl('0',6-len(allt(x)))
		
	ENDPROC

	PROCEDURE Click
		thisform.grid2.visible=.F.
		
	ENDPROC

	PROCEDURE control_cuentas
		select tabla15
		if val(codigo)<>0
			sele cuencont
			set order to codigo
			seek allt(tabla15.codigo)
			if found()
				if thisform.grabado and nomodi_asi=1
					messagebox("No se puede Modificar la Cuenta Banco de un Asiento Grabado",48,"control")
					*thisform.grid1.column2.text1.setfocus
					thisform.text1.setfocus
					
					sele tabla15
					locate for val(codigo)=0
					if !found()
						append blank
					endif
					thisform.grid1.column1.setfocus
					return .f.
				endif
			endif
			
		endif
		return .t.
	ENDPROC

	PROCEDURE Init
		thisform.grabado=.f.
		
		
		select periodo_actual
		*Set Filter To idsesion=_idsesion and idnumero=_idnumero
		*brow
		go top
		if cerrado="S"
			thisform.xpButton3.visible=.f.
		Endif
		select detacont
		set filte to periodo=_periodosele   && periodo_actual.periodo
		select movicont 
		set filte to periodo=_periodosele  && periodo_actual.periodo
		thisform.fecha1.value=date()
		thisform.nroasiento1.value=space(6)
		thisform.text1.value=space(50)
		thisform.text2.value=space(6)
		thisform.text3.value=space(3)
		thisform.text4.value=0.00
		sele detacont
		go bottom
		thisform.text2.value=nroasiento
		thisform.nroasiento1.value=Ceros(Str(Val(nroasiento)+1,6),6)
		Thisform.nroASIENTO1.Visible= .F.
		
		Thisform.xpbutton3.Visible = .F.
		Thisform.xpbutton4.Visible = .F.
		Thisform.xpbutton5.Visible = .F.
	ENDPROC

	PROCEDURE BANCO1.GotFocus
		for k=1 to 3
			keyboard'{SHIFT+RIGHTARROW}'
		endfor
	ENDPROC

	PROCEDURE BANCO1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
				do form busqueda with 'bancos',allt(this.value)+chr(nkeycode),"Codigo","Nombre";
				,"Tipo_Banco","=","S","Codigo","Nombre" to this.value
		endif
	ENDPROC

	PROCEDURE BANCO1.LostFocus
		if val(this.value)#0
		
			
			this.value=ceros(this.value,3)
			
			sele bancos
			set filter to 
			set order to codigo
			seek this.value
			if found()
				If tipo_banco="S"
		
					thisform.txtnombanco.Value=nombre
		
					*******			
				Else	
					messagebox("Esta Cuenta no es bancaria",48,"Control")
					thisform.txtnombanco.Value=''
		
				EndIf	
			else
					thisform.txtnombanco.Value=''
			endif
			thisform.refresh
		endif
	ENDPROC

	PROCEDURE b_buscar.Click
		
		Select aux_movbanco
		TableRevert(.t.)
		
		Select *,Recno() as registro from movimientos_bancos where val(codigo_banco)=Val(thisform.banCO1.value);
		and Iif(thisform.txtdesde.value<>{},fecha_emision=thisform.txtdesde.Value,.t.);
		and tipo_movimiento=Iif(thisform.tiPO.debitos.Value=1,'004','003');
		and Val(num_concilia)=0;
		and Val(asiconci)=0 into cursor sinasientos;
		order by fecha_emision
		
		
		Select sinasientos
		
		
		If Reccount()>0
			Scan
				   m.registro=recno()
				   scatter memvar
				   m.nrocupon=m.numero_comprobante
				   m.fecemi=m.fecha_emision
				   m.fecven=m.fecha_vencimiento
				   _f=Ceros(Str(Day(m.FecVen),2),2)+"/"+Ceros(Str(Month(m.FecVen),2),2)+"/"+Ceros(Str(Year(m.FecVen),4),4) 
				   m.fecpag=m.fecha_ingreso
				   m.tipo_mov=m.tipo_movimiento
				   if m.debe_haber='H'
				   		m.haber=importe
				   		m.debe=0.00
				   		*m.saldo=m.saldo+(m.debe-m.haber)
				   else
				   		m.debe=m.importe
				   		m.haber=0.00
				   		*m.saldo=m.saldo+(m.debe-m.haber)
				   endif
				   select movimientos_banco
				   set order to codigo
				   seek m.tipo_movimiento
				   m.tipo=nombre
				   m.detalle=Alltr(m.tipo)+" - "+alltr(m.detalle)
				   if m.tipo_mov="001"
				   		m.detalle=Alltr(m.tipo)+" Vto: "+_f+" "+alltr(m.detalle)	
				   else
		   			    m.detalle=Alltr(m.tipo)+" - "+alltr(m.detalle)	
				   endif
				   select aux_movbanco
				   append blank
				   gather memvar
			EndScan
			
			Select aux_movbanco
			Go top
			thisform.grid4.column1.text1.Setfocus
		Else
			MessageBox("No hay Datos para Mostrar",48,"Control")
		endif
		thisform.Refresh 
	ENDPROC

	PROCEDURE Command1.Click
		if thisform.text5.value+thisform.text6.value=0
