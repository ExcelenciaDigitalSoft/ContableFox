	PROCEDURE Xpbutton2.Click
		if messagebox('Desa Exportar a Excel ',36,'Asistente')=6
			sele transfe,fecha_emision,detalle, sum(importe) as total ;
			from movimientos_bancos ;
			where Betw(fecha_emision,thisform.text2.value,thisform.text3.value) and tipo_movimiento="006" and debe_haber="D" and val(transfe)#0 ;
			order by transfe,fecha_emision ;
			group by transfe,fecha_emision ;
			into cursor consulta
			select consulta
			go top
			scan
				scatter memvar
				sele listado_transferencias
				append blank
				gather memvar	
				sele consulta
			endscan
			excel("listado_transferencias")
		endif	
		
	ENDPROC

