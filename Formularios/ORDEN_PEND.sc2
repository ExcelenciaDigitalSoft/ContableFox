					_msj=''
					scan
						_msj=_msj + dtoc(Fecha)+'-'
					endscan
					messagebox("Hay Reposiciones de Fondo Fijo pendiente de Cierre,"+chr(13)+"el Cajero debe realizar la Rendición antes de Reponer Nuevamente"+chr(13)+"Fechas a Rendir: "+_msj,48,"Control")
