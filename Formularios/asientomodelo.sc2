	PROCEDURE ceros
		parameter cod
		return allt(cod)+repl('0',6-len(allt(cod)))
	ENDPROC

	PROCEDURE ceros2
		parameter cod
		return repl('0',3-len(allt(cod)))+allt(cod)
	ENDPROC

	PROCEDURE Grid1.Column1.Text1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
			do form busqueda with 'cuencont','','Codigo','Nombre' to this.value
		endif
		if nkeycode=13 and this.value=space(6)
			thisform.detalle1.setfocus
		endif
	ENDPROC

	PROCEDURE Grid1.Column1.Text1.LostFocus
		if val(this.value)#0
			this.value=ceros(this.value,6)
			sele cuencont
			set order to codigo
			seek this.value
			if found()
				repl tabla15.detalle with nombre
			else
				if val(thisform.grid1.column1.text1.value)#0
					=messagebox('Cuenta No encontrada',48,'')		
				endif	
			endif
			sele tabla15
		endif	
	ENDPROC

	PROCEDURE Grid1.Column3.Text1.GotFocus
		thisform.labelcambio.visible=.t.
	ENDPROC

	PROCEDURE Grid1.Column3.Text1.LostFocus
		thisform.labelcambio.visible=.f.
	ENDPROC

	PROCEDURE NROMODELO1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,120)
		*	sele asiemode
		*	set order to detalle
		*	set near on
		*	seek allt(thisform.nromodelo1.value)+chr(nkeycode)
		*	set near off
		*	thisform.grid2.visible=.t.
		*	thisform.grid2.refresh
			if betw(nkeycode,65,90) or betw(nkeycode,97,120)
				do form busqueda with 'asiemode','','nromodelo','detalle',"debehaber","#","Z","","detalle" to this.value
			endif
		endif
	ENDPROC

	PROCEDURE NROMODELO1.LostFocus
		if val(this.value)#0
			this.value=thisform.ceros2(this.value)
			select tabla15
			=tablerevert(.t.)
			sele asiemode
			set order to nromodelo
			seek this.value
			thisform.detalle1.value=detalle	
			do while not eof() and nromodelo=this.value
				scatter memvar
				m.registro=recno()
				sele cuencont
				set order to codigo
				seek m.codigo
				m.detalle=nombre
				sele tabla15
				appe blan
				gather memvar
				sele asiemode
				skip
			enddo
			sele tabla15
			if recc()=0
				appe blan
			endif	
			thisform.grid1.refresh
			thisform.grid1.column1.text1.setfocus
		endif
	ENDPROC

	PROCEDURE Xpbutton1.Click
