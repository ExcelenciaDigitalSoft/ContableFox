	ENDPROC

	PROCEDURE Xpbutton2.Click
		if messagebox('Confirma grabar datos',36,'Grabar')=6 
			sele tabla15
			go top
			do while not eof()
				scatter memvar
				m.nromodelo=thisform.nromodelo1.value		
				m.detalle=thisform.detalle1.value
				select asiemode
				if val(m.codigo)#0
					if m.registro=0
						if m.debehaber#"E"
							appe blan
							gather memvar
						endif	
					else
						go record m.registro
						if m.debehaber#"E"
							gather memvar
						else
							dele	
						endif	
					endif
				endif	
				sele tabla15		
				skip
			enddo
			=tablerevert(.t.)
			thisform.nromodelo1.value = space(3)
			thisform.detalle1.value = space(50)
			thisform.nromodelo1.setfocus
		endif	
	ENDPROC

	PROCEDURE Xpbutton3.Click
		if 	messagebox('Eliminar Asiento Modelo',36,'Eliminar ')=6
			sele asiemode
			seek thisform.nromodelo1.value
			do while not eof() and nromodelo=thisform.nromodelo1.value
				dele
				skip
			enddo
			sele tabla15
			=tablerevert(.t.)
			thisform.nromodelo1.value=space(3)
			thisform.detalle1.value=space(50)
		endif	
		thisform.nromodelo1.setfocus
		
	ENDPROC

