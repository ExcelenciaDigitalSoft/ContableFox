	PROCEDURE pasar
		If Val(ThisForm.TxtCodigo.Value)#0
			Select Menu_Osp
			Set Order To Id
			nBuscar=Round(Val(Substr(ThisForm.SelCar.SelectedItem.Key,2)),0)
			Seek nBuscar
			If Found()
				If Alltrim(Prompt)#"\-"
					Select Aux_Niveles
					Set Order To IdPAd
					Seek Menu_Osp.IdPad
					If !Found()
						Append Blank
					EndIf
					Replace Nivel 		With ThisForm.TxtCodigo.Value
					Replace Nombre 		With ThisForm.TxtNombre.Value
					Replace Clave 		With Menu_Osp.Clave
					Replace Padre		With Menu_Osp.Padre
					Replace NomClave 	With Menu_Osp.Prompt
					Replace IdPad 		With Menu_Osp.IdPad
				Endif
			EndIf
			Select Aux_Niveles
			Set Order To CLAVE	
			ThisForm.List1.Refresh
			ThisForm.List1.SetFocus
		EndIf
	ENDPROC

	PROCEDURE ver_nivel
		Select Aux_niveles
		=TableRevert(.T.)
		Select Niveles
		Set Order To Nivel
		Seek ThisForm.TxtCodigo.Value
		Do While Not Eof() And Nivel=ThisForm.TxtCodigo.Value
			Scatter Memvar
			m.Registro=Recno()
			Select Aux_Niveles
			Append Blank
			Gather Memvar
			Select Niveles
			Skip
		EndDo
		Select Aux_Niveles
		Set Order To CLAVE   && PADRE+CLAVE
		ThisForm.List1.Refresh
		ThisForm.List1.SetFocus
	ENDPROC

	PROCEDURE cmdGrabar.Click
		If Val(ThisForm.txtCodigo.Value)=0
			=MessageBox("Asignele un código al Nivel",48,"Control")
			ThisForm.txtCodigo.SetFocus
			Return
		EndIf
		If Len(Alltrim(ThisForm.txtNombre.Value))=0
			=MessageBox("Asignele un Nombre al Nivel",48,"Control")
			ThisForm.txtNombre.SetFocus
			Return
		EndIf
		If Messagebox('¿Confirma Grabar Datos?',36,'Grabar')=6
			Set Deleted off
			Select Aux_Niveles
			Scan
				Scatter Memvar
				m.Nombre=ThisForm.txtNombre.Value
				If m.Registro=0
					If !Deleted()
						Select Niveles			
						Append Blank
						Gather Memvar
					Endif
				Else
					If !Deleted()
						Select Niveles
						Go Record m.Registro
						Gather Memvar
					Else
						Select Niveles
						Go Record m.Registro
						Delete		
					Endif
				Endif
			Endscan
			Set Deleted On
			=MessageBox("Los datos Fueron Grabados",48,"Control")
			Store "" To ThisForm.txtCodigo.Value,ThisForm.txtNombre.Value
			ThisForm.txtCodigo.SetFocus
		Endif
		ThisForm.Refresh
	ENDPROC

