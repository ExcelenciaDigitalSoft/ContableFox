	PROCEDURE carga
		ThisForm.SelCar.Nodes.Clear()
		Select Menu_Osp
		Set Order To Menu
		Go Top
		Do While Not Eof()
			m.Registro = Recno("Menu_Osp")
			_clave="Z"+Alltrim(Str(IdPad))
			ThisForm.SelCar.Nodes.Add( , ,  _clave, Prompt , 1, 1)
			ThisForm.SelCar.Nodes(_clave).Expanded = .T.
			ThisForm.SelCar.Nodes(_clave).Tag = 1
			Set Order To Clave
			ThisForm.Expande(_clave,Clave)
			Set Order To Menu
			Go m.Registro In Menu_Osp
			Skip 1 In Menu_Osp
		Enddo
		ThisForm.Refresh()
		
	ENDPROC

	PROCEDURE expande
		Lparameters m.IdPad,m.Padre
		Local m.Registro, m.Nombre, m.CampoPadre, m.Imagen
		Select Menu_Osp
		Set Order To Padre
		Seek m.Padre
		Do While !Eof("Menu_Osp") And Padre = m.Padre
			m.Registro = Recno("Menu_Osp")
			_clave="Z"+Alltrim(Str(IdPad))
			ThisForm.SelCar.Nodes.Add(m.IdPad, 4, _clave, Prompt, 2, 3)
			ThisForm.SelCar.Nodes(_clave).Expanded = .F.
			ThisForm.Expande(_clave,Clave)
			Go m.Registro In Menu_Osp
			Skip 1 In Menu_Osp
		Enddo
		
	ENDPROC

