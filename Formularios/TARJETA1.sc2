						m.fecha_emis=m.fecha_emision
						select deposito
						append blank
						gather memvar
						select movimientos_bancos
						skip
					enddo
				endif
				thisform.depositos1.grid1.refresh
				*//Carga el detalle de los Cheques de Terceros//*
				select chequeter
				set order to pagotarjet
				seek this.value
				if found()
					do while not eof() and pagotarjeta=this.value
						m.regi=recno()
						scatter memvar
						select aux_terceros
						append blank
						gather memvar
						sele chequeter
						skip
					enddo
				endif
				thisform.chequeter1.grid1.refresh	
				thisform.grid2.column1.enabled=.f.
			else
				thisform.text1.enabled=.t.
				thisform.text2.enabled=.t.
				thisform.text3.enabled=.t.
				thisform.text4.enabled=.t.
				thisform.text5.enabled=.t.
				thisform.text6.enabled=.t.
				thisform.text7.enabled=.t.
				thisform.text8.enabled=.t.
				thisform.codigo1.value=space(2)
				thisform.fecha.value=date()
				thisform.importe.value=0.00
				thisform.efectivo1.value=0.00
				thisform.text1.value=0.00
				thisform.text2.value=0.00
				thisform.text3.value=0.00
				thisform.text4.value=0.00
				thisform.text5.value=0.00
				thisform.text6.value=0.00
				thisform.text7.value=0.00
				thisform.text8.value=0.00
				thisform.codigo1.enabled=.t.
				thisform.fecha.enabled=.t.	
				thisform.command2.enabled=.t.											
				thisform.grid2.column1.enabled=.t.
			endif
			thisform.grid2.refresh
		endif
	ENDPROC

ENDDEFINE
