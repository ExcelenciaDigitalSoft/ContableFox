	PROCEDURE CODIGO1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if nkeycode=28
			do form busqueda with 'tarjetas','','codigo','nombre' to this.value
		endif
	ENDPROC

	PROCEDURE CODIGO1.LostFocus
		if val(this.value)#0 
			this.value=ceros(this.value,2)
			select tarjetas
			set order to codigo
			seek this.value
			if found()
				thisform.labelnombre.caption=nombre
				select movtar
				set order to pago
				seek this.value
				if found()
					do while not eof()and codtar=this.value
						scatter memvar
						m.r=recno()
						m.numero=m.numfac
						m.fecha=m.fecing
						m.total=m.importe
						m.copiada=0
						select aux_facven
						append blank
						gather memvar
						select movtar
						skip
					enddo
				else
					=messagebox('No Hay Cupones para Presentar de '+tarjetas.nombre,48,'Control')	
					this.value=space(3)
					thisform.labelnombre.caption=''
					thisform.text9.setfocus
				endif	
			else
				if lastkey()=13 or lastkey()=24 or lastkey()=9
					=messagebox('Codigo NO Existente',48,'Control')	
					thisform.text9.setfocus
				endif	
			endif	
			thisform.grid2.refresh
			thisform.grid2.column1.text1.setfocus
		endif
		thisform.label15.visible=.f.
	ENDPROC

