	ENDPROC

	PROCEDURE Command2.Click
		if messagebox('Confirma grabar datos',36,'Grabar')=6 &&si en movmutual el tipo=1 mutual y numfac=al numero de cupon tipo=2 cheque ,numfac=nrocomprobante tipo=3efectivo
			select pagotarjeta
			set order to numero
			seek thisform.text9.value
			if !found()
				append blank
			endif
			repl numero with thisform.text9.value
			repl codigo with thisform.codigo1.value
			repl fecha with thisform.fecha.value
			repl bruto with thisform.importe.value
			repl efectivo with thisform.efectivo1.value
			repl desporfi with thisform.text1.value
			repl retper with thisform.text2.value
			repl retuso with thisform.text3.value
			repl iva with thisform.text4.value
			repl desporco with thisform.text5.value
			repl importe with thisform.text6.value
			repl deposito with thisform.text7.value
			repl cheque with thisform.text8.value
			select aux_facven
			go top
			do while not eof()
				if copiada=1
					select movtar
					go record aux_facven.r
					repl fecpre with thisform.fecha.value
					repl pagonumero with thisform.text9.value
				endif	
				select aux_facven
				skip
			enddo
			*//Grabo depositos//*
			select deposito
			go top
			do while not eof() and val(codigo_ban)#0
				scatter memvar
				m.codigo_banco=m.codigo_ban
				m.numero_comprobante=m.numero_com
				fecha_emision=fecha_emis
				fecha_vencimiento=fecha_emis
				m.tipo_movimiento='002'
				m.debe_haber='H'
				m.recibo=space(6)
				m.tarjeta=thisform.text9.value
				select movimientos_bancos
				if m.registro=0
					append blank
					gather memvar			
				else
					go record m.registro
					if m.importe#0
						gather memvar				
					else
						dele	
					endif	
				endif	
				select deposito
				skip
			enddo
			*//Grabo Cheques de Tercero
			sele aux_terceros
			go top
			do while not eof() and val(comproban)#0
				scatter memvar
				m.pagotarjeta=thisform.text9.value
				m.detalle=alltr(m.detalle)+'Liq. de Tarjeta N°: '+thisform.text9.value
				m.recibo=space(6)
				select chequeter
				if m.regi=0
					append blank
					gather memvar
				else
					go record (m.regi)
					if m.importe=0
						dele
					else
						gather memvar
					endif	
				endif
				select aux_terceros
				skip
			enddo			
			select aux_facven
			=tablerevert(.t.)
			select aux_terceros
			=tablerevert(.t.)
			select deposito
			=tablerevert(.t.)
			thisform.labelnombre.caption=''
			thisform.importe.value=0
			thisform.text1.value=0
			thisform.text2.value=0
			thisform.text3.value=0
			thisform.text4.value=0
			thisform.text5.value=0
			thisform.text6.value=0
			thisform.text7.value=0
			thisform.text8.value=0
			thisform.efectivo1.value=0
			thisform.codigo1.value=space(2)
			select pagotarjeta
			set order to numero
			go bott
			thisform.ultimo.caption=numero
			thisform.text9.value=ceros(str(val(numero)+1,6),6)
			thisform.text9.setfocus
			thisform.refresh
		endif	
	ENDPROC

