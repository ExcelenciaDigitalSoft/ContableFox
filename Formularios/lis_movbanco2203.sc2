	PROCEDURE Init
		thisform.fecha_inicio={}	
		thisform.saldo_inicial=0.00
		thisform.banco1.value= space(2)
		thisform.text2.value=date()
		
	ENDPROC

	PROCEDURE ver
		lparameters _tipo
			TB="S"
			fil1="select codigo_banco as banco,numero_comprobante ,Tipo_movimiento as Tipo_Mov,Fecha_emision as Fec_Emi,fecha_vencimiento as fec_Ven ,fecha_ingreso as fec_ing ,importe ,detalle,pagprov as orden,Space(100) as provcta from Movimientos_Bancos"
			fil2="where Tipo_banco=TB and codigo_banco=thisform.Banco1.value and Tipo_movimiento=thisform.tipo.value and codmodi=0"  && agrego codmodi=0 para que solo muestre los movimientos que ingresaron al banco o pueden ingresar al banco, es decir pueden ser marcados por una conciliación.
			
			if thisform.orden.fechaemision.value=1
				order1="order by  Fecha_emision"
			Else
				If thisform.orDEN.fechavto.Value=1
					order1="order by  fecha_vencimiento"
				Else
					order1="order by  fecha_ingreso"
				EndIf
			EndIf
			
			
			
			if thisform.o1.o1.value=1
				fil3="and betw(Fecha_emision,thisform.text1.value,thisform.text2.value)"
			Else
				If thisform.o1.o2.Value=1
					fil3="and betw(fecha_vencimiento,thisform.text1.value,thisform.text2.value)"
				Else
					fil3="and betw(fecha_ingreso,thisform.text1.value,thisform.text2.value)"
				endif
			endif
			do case
				case thisform.o2.o1.value=1
					fil4="and fecha_ingreso # {}"
				case thisform.o2.o2.value=1
					fil4="and fecha_ingreso = {}"
				case thisform.o2.o3.value=1
					fil4="and (fecha_ingreso >=thisform.text1.value and fecha_ingreso <=thisform.text2.value and fecha_ingreso<>{})"
					&&fil4=""
			endcase	
			filtro=fil1+" "+fil2+" "+fil3+" "+fil4+" "+order1+" into cursor consulta readwrite"
			&filtro
			
				select consulta
			Scan
				If Val(orden)<>0
					Select orden
					Set Order To nroord
					Seek consulta.orden
					If Found()
						Replace consulta.provcta with Alltrim(detalle)
					endif
				endif
			endsca
			Select consulta
			
			select consulta
			if _tipo=1
				Repor form lis_movbanco NoCon Prev
			else
				excel('consulta')
			endif
	ENDPROC

	PROCEDURE BANCO1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
				do form busqueda with 'bancos',allt(this.value)+chr(nkeycode),"Codigo","Nombre";
				,"Tipo_Banco","=","S","Codigo","Nombre" to this.value
		endif
	ENDPROC

	PROCEDURE BANCO1.LostFocus
		if val(this.value)#0
			this.value=ceros(this.value,3)
			sele bancos
			set filter to 
			set order to codigo
			seek this.value
			if found()
				If tipo_banco="S"
					thisform.nombre=nombre
					thisform.cuenta=cuenta	
					thisform.label1.caption=nombre
					thisform.label6.caption='Cuenta N° '+cuenta
					thisform.cuen=cuenta
					thisform.saldo_inicial=sal_inicio
					thisform.fecha_inicio=fecha
				Else	
					messagebox("Esta Cuenta no es bancaria",48,"Control")
					thisform.label1.caption=""
					thisform.label6.caption=""
					thisform.cuen=""
					this.value=""
					thisform.saldo_inicial=0.00
					thisform.fecha_inicio={}			
					node
				EndIf	
			else
				thisform.nombre=''
				thisform.cuenta=''	
				thisform.label1.caption=''
				thisform.label6.caption=''	
				thisform.cuen=''
			endif
			thisform.refresh
		endif
	ENDPROC

