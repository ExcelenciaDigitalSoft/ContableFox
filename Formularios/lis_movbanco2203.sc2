	ENDPROC

	PROCEDURE Tipo.LostFocus
		this.backcolor=c_salida
		if val(this.value)#0
			this.value=ceros(this.value,3)
			select movimientos_banco
			set order to codigo
			seek this.value
			if found()
				thisform.label7.caption=nombre
			else
				thisform.label7.caption=""
				messagebox("Código de Movimiento Bancario Incorrecto",48,"Control")	
			endif
		else
			if lastkey()=13 or lastkey()=24 or lastkey()=9
				Messagebox("Debe Cargar Código de Movimiento Bancario",48,"Control")
				node
			endif	
		endif
	ENDPROC

	PROCEDURE Xpbutton1.Click
		select aux_movbanco
		=tablerevert(.t.)
		thisform.release
	ENDPROC

	PROCEDURE Xpbutton2.Click
		if messagebox('Desea Exportar a Excel ',36,'Asistente')=6
			*excel("Aux_MovBanco")
			thisform.ver(2)
		endif
	ENDPROC

	PROCEDURE Xpbutton4.Click
		if messagebox('Desea Visualizar Informe',36,'Informe')=6
			thisform.ver(1)
		endif
		Return
		
	ENDPROC

ENDDEFINE
