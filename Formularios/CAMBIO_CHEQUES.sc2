		if val(this.value)#0
			this.value=ceros(this.value,3)
			sele bancos
			set filter to 
			set order to codigo
			seek this.value
			if found()
				If tipo_banco="S"
					thisform.anular_cheques.txtnombanco.Value=nombre
				Else	
					messagebox("Esta Cuenta no es bancaria",48,"Control")
					thisform.anular_cheques.txtnombanco.Value=''			
					this.Value=''
				EndIf	
			else
				thisform.anular_cheques.txtnombanco.Value=''
			endif
			thisform.refresh
		endif
	ENDPROC

	PROCEDURE anular_cheques.Command1.Click
		*--- Controlar que el cheque exista y no este usado, o ya anulado.
		_error=0
		If thisform.controlar_anulacion_cheque()
				*-- Controlar que tenga cargado el motivo.
			If Len(allt(thisform.anular_cheques.txtmotivo.value))=0
				MessageBox("Ingrese el Motivo por el cual realiza la Anulación de Cheque",48,"Control")
				thisform.anular_cheques.txtmotivo.SetFocus
				Return .f.
			EndIf
				
			If MessageBox("Está seguro que desea Anular el Cheque",36,"Confirmacion")=6
		
			Try
		
					Select movimientos_bancos
					Append Blank
		
					Replace cod_bloqueo with '001'  && Anulado  - Cheque que todavia no se carga en el sistema.
					Replace fec_bloqueo with Datetime()
					Replace mot_bloqueo with Alltrim(thisform.anular_cheques.txtmotivo.Value)
					Replace op_bloqueo with val(cod_op)
		
					Replace numero_comprobante with thisform.anular_cheques.txtnumero.Value
					Replace codigo_banco with thisform.anular_cheques.banco.Value
					Replace chequera with thisform.anular_cheques.txtchequera.Value
					Replace fecha_emision with {}
					Replace fecha_vencimiento with {}
					Replace tipo_movimiento with '001'			
					Replace debe_haber with 'H'
					Replace tipo_banco with 'S'
					Replace importe with 0
					Replace det_codban with thisform.anular_cheques.txtnombanco.Value
					Replace detalle with 'Anulado - ' +Alltrim(mot_bloqueo)
		
					Scatter memvar
					_nroregnuevo=Recno()	
					
					*-- Creo un registro con los datos del cheque viejo en Cambios Cheques
					Select cambios_cheques
					Append Blank
					Gather memvar
					Replace nroregistro_nuevo with _nroregnuevo
					Replace tipo with 2  && Viejo
					Replace nom_bloqueo with 'Bloqueado por Anulación'
					*---------------------------------------------------------------------------
		
				
			    CATCH to Err
			        MESSAGEBOX("No se pudieron Grabar los Datos, Error: "+Err.Message+" Linea: "+ALLTRIM(STR(Err.Lineno)),48,"Control")
			        _error=1
			    FINALLY
			       
			    ENDTRY
			   
			    IF _error=0
		   	   		MESSAGEBOX("La Anulación se realizó Correctamente, a Continuación se imprimirá el Comprobante ",64,'Aviso')
		   	        Report Form anulacion_cheques NOCONSOLE PREVIEW 
			        thisform.limpiar()
			    EndIf
		    	
			EndIf
		endif
		
		
	ENDPROC

	PROCEDURE anular_cheques.txtnumero.LostFocus
		thisform.controlar_anulacion_cheque()
	ENDPROC

