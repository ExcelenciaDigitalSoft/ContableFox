	PROCEDURE cambiar_cheques.Command1.Click
		*--- Controlar Totales 
		_tot=0
		Select aux_cambios
		scan
			If Val(codigo_banco)<>0
				If Val(numero_comprobante)=0
					MessageBox("Ingrese el Nro de Comprobante o Borre el Banco para Eliminar el Registro",48,"Control")
					thisform.grdAux_cambios.column1.text1.SetFocus 
					return
				EndIf
				If fecha_emision={}
					MessageBox("Ingrese la Fecha de Emisión del Nuevo Cheque",48,"Control")
					thisform.grdAux_cambios.column5.text1.SetFocus 
					return
				endif
				If fecha_vencimiento={}
					MessageBox("Ingrese la Fecha de Vencimiento del Nuevo Cheque",48,"Control")
					thisform.grdAux_cambios.column6.text1.SetFocus 
					return
				endif
				If importe=0
					MessageBox("Ingrese Importe del Nuevo Cheque",48,"Control")
					thisform.grdAux_cambios.column7.text1.SetFocus 
					return
				endif
				_tot=_tot + importe
			EndIf
		EndScan
		
		If _tot<>thisform.cambiar_cheques.txtimporte.Value
			MessageBox("No coincide el Importe del Cheque a Reemplazar con la Suma de los Cheques Nuevos",48,"Control")
			thisform.grdAux_cambios.column7.text1.SetFocus
			Return
		EndIf
		
		*--- Controlar que el cheque exista y no este usado, o ya anulado.
		_error=0
		If thisform.controlar_cambio_cheque2()
				*-- Controlar que tenga cargado el motivo.
			If Len(allt(thisform.cambiar_cheques.txtmotivo2.value))=0
				MessageBox("Ingrese el Motivo por el cual realiza el Cambio de Cheque",48,"Control")
				thisform.cambiar_cheques.txtmotivo2.SetFocus
				Return .f.
			EndIf
				
			If MessageBox("Está seguro que desea Reemplazar el Cheque",36,"Confirmacion")=6
		
			Try
				If (thisform.cambiar_cheques.txtrecno.value)<>0
					Select movimientos_bancos
					Go thisform.cambiar_cheques.txtrecno.value
					
					Replace cod_bloqueo with '002'  && Rechazado
					Replace fec_bloqueo with Datetime()
					Replace mot_bloqueo with Alltrim(thisform.cambiar_cheques.txtmotivo2.Value)
					Replace op_bloqueo with val(cod_op)
					Scatter memvar
					if len(allt(m.det_codban))=0
						m.det_codban= thisform.cambiar_cheques.txtnombanco.Value
					endif
					Delete 
					*--- && Creo un registro en blanco, con los datos del cheque rechazado solamente.  En la conciliación no lo deberá mostrar.
					Append Blank  
					Replace numero_comprobante with m.numero_comprobante
					Replace codigo_banco with m.codigo_banco
					Replace chequera with m.chequera
					Replace fecha_emision with m.fecha_emision
					Replace fecha_vencimiento with m.fecha_vencimiento
					Replace importe with 0 && m.importe  (para que no sume en los informes)
					Replace tipo_movimiento with m.tipo_movimiento
					Replace debe_haber with m.debe_haber
					Replace detalle with Alltrim(detalle)+' -Bloqueado- '+allt(m.mot_bloqueo)
					
					Replace tipo_banco with m.tipo_banco
					Replace det_codban with allt(m.det_codban)
					Replace cod_bloqueo with m.cod_bloqueo
					Replace fec_bloqueo with m.fec_bloqueo
					Replace mot_bloqueo with m.mot_bloqueo
					Replace op_bloqueo with m.op_bloqueo		
		
					*-- Creo un registro con los datos del cheque viejo en Cambios Cheques
					Select cambios_cheques
					Append Blank
					Gather memvar
					Replace nroregistro with thisform.cambiar_cheques.txtrecno.value 
					Replace tipo with 1  && Viejo
					Replace nom_bloqueo with 'Bloqueado por Reemplazo'
					
		
					*/--- CHEQUE NUEVO  ----------------------------------------------------------------
					Select aux_cambios
					Scan
						If Val(codigo_banco)<>0 and Val(numero_comprobante)<>0
							*-- && Creo un registro nuevo con los datos del viejo registro con el cheque nuevo
							Select movimientos_bancos
							go thisform.cambiar_cheques.txtrecno.value 
							Append Blank  
							Gather memvar
							Replace numero_comprobante with aux_cambios.numero_comprobante
							Replace codigo_banco with 		aux_cambios.codigo_banco
							Replace chequera with 			aux_cambios.chequera
							Replace fecha_emision with 		aux_cambios.fecha_emision
							Replace fecha_vencimiento with 	aux_cambios.fecha_vencimiento
							Replace importe with 			aux_cambios.importe
							Scatter memvar
							Replace cod_bloqueo with ''  
							Replace fec_bloqueo with {}
							Replace mot_bloqueo with ''
							Replace op_bloqueo with 0
							_nroregnuevo=Recno()	
							
							
							Select cambios_cheques
							Append Blank
							Gather memvar
							Replace nroregistro with thisform.cambiar_cheques.txtrecno.value 
							Replace nroregistro_nuevo with _nroregnuevo  && registro creado
							Replace tipo with 2  && Viejo
							Replace nom_bloqueo with 'Bloqueado por Reemplazo'
						EndIf
					EndScan
					
					*---------------------------------------------------------------------------
				EndIf
				
			    CATCH to Err
			        MESSAGEBOX("No se pudieron Grabar los Datos, Error: "+Err.Message+" Linea: "+ALLTRIM(STR(Err.Lineno)),48,"Control")
			        _error=1
			    FINALLY
			       
			    ENDTRY
			   
			    IF _error=0
		   	        MESSAGEBOX("El Cambio se realizó Correctamente, a continuación se imprimirá el Comprobante ",64,"Control")
		   	        Select aux_cambios
		   	        *Set Filter To Val(codigo_banco)<>0 and Val(numero_comprobante)<>0
			        Report Form cambio_cheques2 NOCONSOLE PREVIEW 
			        *Set Filter To 
			        thisform.limpiar()
			    EndIf
		    	
			EndIf
		endif
	ENDPROC

