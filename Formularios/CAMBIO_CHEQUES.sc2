	PROCEDURE controlar_anulacion_cheque
		_resultado=.t.
		
		If val(thisform.anular_cheques.banco.value)=0
			MessageBox("Ingrese el Código de Banco",48,"Control")
			thisform.anular_cheques.banco.SetFocus
			Return .f.
		EndIf
		
		If val(thisform.anular_cheques.txtnumero.value)=0
			MessageBox("Ingrese el Número de Cheque a Reemplazar",48,"Control")
			thisform.anular_cheques.txtnumero.SetFocus
			Return .f.
		EndIf
		
		If Val(thisform.anular_cheques.txtnumero.Value)<>0
			
			thisform.anular_cheques.txtnumero.Value=ceros(thisform.anular_cheques.txtnumero.Value,10)
			
				Select * from movimientos_bancos where codigo_banco=thisform.anular_cheques.banco.Value and;
				numero_comprobante=thisform.anular_cheques.txtnumero.Value and tipo_banco='S' and tipo_movimiento='001';
				into cursor mov_cheque
				
				Select mov_cheque
				If Reccount()>0
					If Val(cod_bloqueo)<>0
						MessageBox("El Número de Cheque se encuentra Bloqueado por: "+Alltrim(mot_bloqueo),48,"Control")
					else
						MessageBox("El Número de Cheque ya se encuentra Utilizado",48,"Control")
					endif
					Return .f.			
				EndIf
				
				*-- Recuperar si es un cheque que esta en una chequera habilitada.
				Select * from chequeras where banco=thisform.anular_cheques.banco.Value and habilitado=1;
				and Val(thisform.anular_cheques.txtnumero.value)>=Val(desde) and Val(thisform.anular_cheques.txtnumero.value)<=Val(hasta);
				into cursor esta_chequera
				Select esta_chequera
				If Reccount()>0
					Go top
					thisform.anular_cheques.txtchequera.Value=codigo
				Else
					MessageBox("El Número de Cheque No se Encuentra en Ninguna Chequera habilitada",48,"Control")
					Return .f.	
				EndIf
		EndIf
		Return _resultado
	ENDPROC

	PROCEDURE controlar_cambio_cheque
		_resultado=.t.
		
		If val(thisform.cambiar_cheques.banco.value)=0
			MessageBox("Ingrese el Código de Banco",48,"Control")
			thisform.cambiar_cheques.banco.SetFocus
			Return .f.
		EndIf
		
		If val(thisform.cambiar_cheques.txtnumero.value)=0
			MessageBox("Ingrese el Número de Cheque a Reemplazar",48,"Control")
			thisform.cambiar_cheques.txtnumero.SetFocus
			Return .f.
		EndIf
		
		thisform.cambiar_cheques.txtimporte.Value=0
		thisform.cambiar_cheques.txtfecha_emision.Value={}
		thisform.cambiar_cheques.txtfecha_vencimiento.Value={}
		thisform.cambiar_cheques.txtrecno.Value=0
		thisform.cambiar_cheques.txtchequera.Value=''
		thisform.cambiar_cheques.txtnro_comprobante.Value=''
		
		
		If Val(thisform.cambiar_cheques.txtnumero.Value)<>0
			
			thisform.cambiar_cheques.txtnumero.Value=ceros(thisform.cambiar_cheques.txtnumero.Value,10)
			
			Select *,Recno()as nroregistro from movimientos_bancos where codigo_banco=thisform.cambiar_cheques.banco.Value and;
			numero_comprobante=thisform.cambiar_cheques.txtnumero.Value and tipo_banco='S' and tipo_movimiento='001';
			into cursor mov_cheque
			
			Select mov_cheque
			If Reccount()>0
				If Val(num_concilia)=0  && No debe estar conciliado para poder realizar el cambio.
					If Val(cod_bloqueo)=0
						*--- Se puede Cambiar 
						thisform.cambiar_cheques.txtimporte.Value=importe
						thisform.cambiar_cheques.txtfecha_emision.Value=fecha_emision
						thisform.cambiar_cheques.txtfecha_vencimiento.Value=fecha_vencimiento	
						thisform.cambiar_cheques.txtrecno.Value=nroregistro
						thisform.cambiar_cheques.txtchequera.Value=chequera
						thisform.cambiar_cheques.txtnro_comprobante.Value=pagprov
					Else
						MessageBox("El Número de Cheque a Reemplazar se encuentra Bloqueado por: "+Alltrim(mot_bloqueo),48,"Control")
						_resultado=.f.
					EndIf
				Else
					MessageBox("El Número de Cheque a Reemplazar se encuentra Conciliado en la Conciliación Nro: "+Alltrim(num_concilia),48,"Control")
					_resultado=.f.
				EndIf
				
			Else
				MessageBox("El Número de Cheque a Reemplazar no se encuentra Cargado",48,"Control")
				_resultado=.f.
			EndIf
			
			If _resultado=.t.
			
				If val(thisform.cambiar_cheques.txtnumero_nuevo.value)=0
					MessageBox("Ingrese el Número de Cheque Nuevo",48,"Control")
					thisform.cambiar_cheques.txtnumero_nuevo.SetFocus
					Return .f.
				EndIf
				
				*-- Ahora controlo el cheque de ingreso nuevo. 
				thisform.cambiar_cheques.txtnumero_nuevo.Value=ceros(thisform.cambiar_cheques.txtnumero_nuevo.Value,10)
			
				Select * from movimientos_bancos where codigo_banco=thisform.cambiar_cheques.banco_nuevo.Value and;
				numero_comprobante=thisform.cambiar_cheques.txtnumero_nuevo.Value and tipo_banco='S' and tipo_movimiento='001';
				into cursor mov_cheque
				
				Select mov_cheque
				If Reccount()>0
					MessageBox("El Número de Cheque Nuevo ya se encuentra Utilizado",48,"Control")
					Return .f.			
				EndIf
				
				*-- Recuperar si es un cheque que esta en una chequera habilitada.
				Select * from chequeras where banco=thisform.cambiar_cheques.banco_nuevo.Value and habilitado=1;
				and Val(thisform.cambiar_cheques.txtnumero_nuevo.value)>=Val(desde) and Val(thisform.cambiar_cheques.txtnumero_nuevo.value)<=Val(hasta);
				into cursor esta_chequera
				Select esta_chequera
				If Reccount()>0
					Go top
					thisform.cambiar_cheques.txtchequera_nuevo.Value=codigo
				Else
					MessageBox("El Número de Cheque Nuevo No se Encuentra en Ninguna Chequera habilitada",48,"Control")
					Return .f.	
				endif
				
		
				
			endif
			
			
		EndIf
		Return _resultado
	ENDPROC

