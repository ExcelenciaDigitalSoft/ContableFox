	ENDPROC

	PROCEDURE CODIGO1.GotFocus
		for K=1 to 6
			keyb '{SHIFT+RIGHTARROW}'
		endfor
		
	ENDPROC

	PROCEDURE CODIGO1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
			do form busqueda with 'proveedores','','codigo','nombre' to this.value
		endif
	ENDPROC

	PROCEDURE CODIGO1.LostFocus
		if val(this.value)#0
			this.value=ceros(this.value,6)
			select proveedores
			set order to codigo
			seek thisform.codigo1.value
			if found()
				thisform.text5.value=nombre
				if VAl(sector)#2
					messagebox("No es un centro de facturacion")
				Endif
			else
				thisform.text5.value=""
				messagebox("Codigo de Centro de Facturacion Incorrecto",48,"Control")
				this.value=""
			endif	
		else
			if lastkey()=13 or lastkey()=9 or lastkey()=24
				thisform.text5.value="Todos los Proveedores"	
			endif	
		endif
		
	ENDPROC

