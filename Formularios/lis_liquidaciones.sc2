	PROCEDURE Xpbutton2.Click
		if messagebox('Desa Exportar a Excel ',36,'Asistente')=6
			if Val(thisform.codigo1.value)=0 &&Todos los Proveedores
				if val(thisform.text1.value)#0
					select * ,vercod("Tipo_liquidacion",tipo) as NomTipo from liquidaciones ;
					where betw(fecha,thisform.text2.value,thisform.text3.value) and tipo=thisform.text1.value ;
					order by tipo,fecha,proveedor ;
					into cursor Consulta
				else   && Todos los tipos de Ingresos
					select * ,vercod("Tipo_liquidacion",tipo) as NomTipo from liquidaciones ;
					where betw(fecha,thisform.text2.value,thisform.text3.value) ;
					order by tipo,fecha,proveedor ;
					into cursor Consulta
				ENDIF
			else && Filtra Por Proveedor
				if val(thisform.text1.value)#0
					select * ,vercod("Tipo_liquidacion",tipo) as NomTipo from liquidaciones ;
					where betw(fecha,thisform.text2.value,thisform.text3.value) ;
					and tipo=thisform.text1.value and proveedor=thisform.codigo1.value ;
					order by tipo,fecha,proveedor ;
					into cursor Consulta
				else   && Todos los tipos de Ingresos
					select * ,vercod("Tipo_liquidacion",tipo) as NomTipo from liquidaciones ;
					where betw(fecha,thisform.text2.value,thisform.text3.value) and proveedor=thisform.codigo1.value ;
					order by tipo,fecha,proveedor ;
					into cursor Consulta
				ENDIF
			Endif	
			select lis_liquidaciones
			=tablerevert(.t.)
			select consulta
			go top
			scan
				scatter memvar
				select lis_liquidaciones
				append blank
				gather memvar
				select consulta
			endscan
			excel("lis_liquidaciones")
		endif	
	ENDPROC

