	PROCEDURE Xpbutton1.Click
		******* Veo si tiene hijos *******
		Store "" to _niv ,_padre
		Store 0 to _cer
		FOR i=1 TO 12 STEP 2
		  IF val(SUBSTR(thisform.txtcodigo.Value,i,2))=0
		     _cer=_cer+1
		  endif
		endfor 
		_niv = 6 - _cer
		_dig = _niv * 2
		If _dig % 2 # 0
			_dig = _dig + 1
		Endif
		
		_padre = Left(Alltrim(thisform.txtcodigo.Value),_dig)
		_suma = thisform.txtcodigo.Value
		
		*SELECT * from cuentas ;
		WHERE codper=_periodo and  Left(codigo,_dig)=_padre order by codigo;
		into cursor cuen
		SELECT * from cuentas ;
		WHERE Left(codigo,_dig)=_padre order by codigo;
		into cursor cuen
		Select cuen
		If Reccount()>1
			MessageBox("No puede eliminar esta cuenta por que existen cuentas que acumulan el ella",48,"Control")
			Return
		Endif
		If Val(thisform.txtCodigo.Value) = 0
			MessageBox("Debe seleccionar la cuenta que desea eliminar",48,"Control")
			Return
		Endif
		If MessageBox("Esta Seguro que Desea Eliminar la Cuenta :"+Alltrim(Thisform.txtNombre.Value),36,"Control")=6
			If mayoriza(Thisform.txtCodigo.Value,"T")=0
				Select CUENTAS
				Dele
				=TableUpdate(.T.)
				thisform.carga()
				thisform.selcar.Refresh()
			Else
				MessageBox("Esta cuenta tiene movimiento no puede ser eliminada",48,"Control")	
			Endif
		EndIf
	ENDPROC

