	PROCEDURE carga
		thisform.selcar.nodes.clear()
		select cuentas
		*Set Filter To codper=_periodo
		=TableRevert(.t.)
		set orde to menu
		go top
		do while not eof()
			m.registro = recno("cuentas")
			thisform.selcar.nodes.add( , , clave , alltr(nombre)+" - "+cuentas.codigo , 1, 1)
			thisform.selcar.nodes(clave).expanded = .t.
			thisform.selcar.nodes(clave).tag = 1
			set order to clave
			thisform.expande(clave)
			set order to menu
			go m.registro in cuentas
			skip 1 in cuentas
		enddo
		thisform.refresh()
		
	ENDPROC

	PROCEDURE expande
		lparameters m.padre
		local m.registro, m.nombre, m.campopadre, m.imagen
		sele cuentas
		set order to padre
		seek m.padre
		do while !eof("cuentas") and padre = m.padre
			m.registro = recno("cuentas")
			thisform.selcar.nodes.add(m.padre, 4, clave, alltr(nombre)+" - "+cuentas.codigo, 2, 3)
			thisform.selcar.nodes(clave).expanded = .t.
			thisform.expande(clave)
			go m.registro in cuentas
			skip 1 in cuentas
		enddo
		
	ENDPROC

