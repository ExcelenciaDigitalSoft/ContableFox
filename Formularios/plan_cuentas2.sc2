			Set Order To codigo
			Seek sectores.codigo+"0000"
			If Found()
				MessageBox("Esta Cuenta Primaria Ya Existe",48,"Control")
			else	
				Append Blank
				Replace codigo with sectores.codigo+"0000"
				Replace nombre with sectores.nombre
				Replace suma with ""
				Replace acumuladora with "N"
				Replace padre with ""
				Replace clave with "A"+sectores.codigo+"0000"
				=TableUpdate(.t.)
				thisform.carga()
				thisform.selcar.Refresh()
			Endif
		endif	
		
	ENDPROC

