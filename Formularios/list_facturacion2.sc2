	ENDPROC

	PROCEDURE Xpbutton7.Click
		Select aux_det
		=TableRevert(.t.)
		**** Toda lo Facturado (Liquidado o no)****
		if thisform.o.op1.Value=1
			if val(thisform.CODIGO1.Value)#0
				Select * from det_fac where Between(fecha,thisform.textdesde.value,thisform.texthasta.value);
				and centro_fac=Alltrim(thisform.CODIGO1.Value) order by fecha;
				into cursor consulta
			Else
				Select * from det_fac where Between(fecha,thisform.textdesde.value,thisform.texthasta.value);
				order by fecha into cursor consulta
			endif	
		EndIf
		**** Todo lo Liquidado****
		if thisform.o.op2.Value=1
			if val(thisform.CODIGO1.Value)#0
				Select * from det_fac where Between(fecha,thisform.textdesde.value,thisform.texthasta.value);
				and centro_fac=Alltrim(thisform.CODIGO1.Value) and Val(num_liq)#0  order by fecha;
				into cursor consulta
			Else
				Select * from det_fac where Between(fecha,thisform.textdesde.value,thisform.texthasta.value);
				and Val(num_liq)#0  order by fecha into cursor consulta
			endif	
		EndIf
		**** Todo lo Facturado Pendiente de Liquidación *****
		if thisform.o.op3.Value=1
			if val(thisform.CODIGO1.Value)#0
				Select * from det_fac where Between(fecha,thisform.textdesde.value,thisform.texthasta.value);
				and  Val(num_liq)=0 and centro_fac=Alltrim(thisform.CODIGO1.Value) order by fecha;
				into cursor consulta
			Else
				Select * from det_fac where Between(fecha,thisform.textdesde.value,thisform.texthasta.value);
				and  Val(num_liq)=0 order by fecha;
				into cursor consulta
			endif	
		EndIf
		**** Todo (Facturado, Liquidado y Pendientes) ****
		If thisform.o.op4.Value=1
			if val(thisform.CODIGO1.Value)#0
				Select * from det_fac where Between(fecha,thisform.textdesde.value,thisform.texthasta.value);
				order by fecha;
				into cursor consulta
			Else
				Select * from det_fac where Between(fecha,thisform.textdesde.value,thisform.texthasta.value);
				order by fecha;
				into cursor consulta
			endif	
		endif
		Select consulta
		If Reccount()#0
			Go top
			DO while not Eof()
				Scatter memvar
				m.registro=Recno()
				Select aux_det
				Append Blank
				Gather memvar
				Select consulta 
				skip
			EndDo
			Select aux_det
			Report Form list_facturacion noconsole previ
		Else
			MessageBox("No hay registros en ese periodo")		
		endif
		
	ENDPROC

