	PROCEDURE Init
		Select aux_det
		=TableRevert(.t.)
		thisform.textdesde.Value=CTOD("01/"+ceros(str(month(date()),2),2)+"/"+Ceros(Str(year(date()),4),4))
		thisform.texthasta.Value=Date()
		
		
	ENDPROC

	PROCEDURE CODIGO1.GotFocus
		thisform.label6.Visible=.t.
		for k=1 to 6
			keyboard'{SHIFT+RIGHTARROW}'
		endfor
		
	ENDPROC

	PROCEDURE CODIGO1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
			do form busqueda with 'proveedores' to this.value
		EndIf	
		
	ENDPROC

	PROCEDURE CODIGO1.LostFocus
		thisform.label6.Visible=.t.
		if val(this.value)#0 
			this.value=ceros(this.value,6)
			select proveedores
			set order to codigo
			seek this.value
			If found()
				thisform.label5.caption=Alltrim(nombre )
			else
				thisform.label5.caption=""
				messagebox("Codigo de Proveedor incorrecto",48,"Control")	
				this.value=""
				node
			Endif	
		Else
			If Lastkey()=13 or Lastkey()=24 or Lastkey()=9
				thisform.label5.caption="Todos los Centros de Facturación"
			Endif
		EndIf
		
	ENDPROC

	PROCEDURE Texthasta.LostFocus
		If this.Value={}
			MessageBox("Debe ingresar Fecha Hasta")
			this.value=Date() 
			this.setfocus
			return
		endif
	ENDPROC

	PROCEDURE Xpbutton1.Click
