	PROCEDURE Init
		public cuit,cli
		thisform.p=0
		thisform.totreg=0
		select proveedores
		set order to cuit
		go top
		scan
			thisform.totreg=thisform.totreg+1
		endscan
		dimen cuit(thisform.totreg)
		dimen cli(thisform.totreg)
		select proveedores
		set order to cuit
		go top
		for a =1 to thisform.totreg
			if proveedores.cuit#space(11)
				cuit(a)=proveedores.cuit
			else
				cuit(a)=space(11)
			endif	
			cli(a)=proveedores.codigo
			skip 
		endfo
		sele proveedores
		set order to codigo
		go bott
		thisform.labelultimo.caption=codigo
		skip
		thisform.codigo1.setfocus
	ENDPROC

	PROCEDURE CODIGO1.GotFocus
		for K=1 to 6
			keyb '{SHIFT+RIGHTARROW}'
		endfor
		select proveedores
		=tablerevert(.t.)
		thisform.refresh
		
	ENDPROC

	PROCEDURE CODIGO1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
			do form busqueda with 'proveedores','','codigo','nombre' to this.value
		endif
	ENDPROC

	PROCEDURE CODIGO1.LostFocus
		if val(this.value)#0
			Select aux_Porcentaje_Proveedores
			=TableRevert(.t.)
			this.value=ceros(this.value,6)
			sele proveedores
			set order to codigo
			seek this.value
			if !found()
				appe blan
				repl codigo with this.value
				repl sector with "001"
				thisform.label1.caption=""
			else
				if val(proveedores.sector)#0
					thisform.label1.caption=verfield("sectores",proveedores.sector,"nombre")
				else
					thisform.label1.caption=""	
				endif	
				Select Porcentaje_Proveedores
				Set Order To Proveedor
				Seek this.Value
				DO while not Eof() and proveedor=this.Value
					Scatter memvar
					m.registro=Recno()
					Select aux_Porcentaje_Proveedores
					Append Blank
					Gather Memvar
					Select Porcentaje_Proveedores
					skip
				EndDo
			EndIf
			Select aux_Porcentaje_Proveedores
			If Reccount()=0
				Append Blank
			EndIf
			Thisform.Grid1.Refresh	
		else
			if lastkey()=13 or lastkey()=9 or lastkey()=24
				thisform.label1.caption=""
			endif	
		endif
		thisform.refresh
		
	ENDPROC

	PROCEDURE CONDICION1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		
	ENDPROC

	PROCEDURE CUIT1.LostFocus
		if val(this.value)#0
			*if validarcuit(alltr(this.value))
				for a =1 to thisform.totreg
					if (this.value)=cuit(a) and thisform.codigo1.value#cli(a)
						If messagebox('Este Nro. de C.U.I.T. ya Existe.'+chr(13)+;
						"Desea Seguir con la Carga",36,'Seguridad')=7
							nodefault
						EndIf	
					endif
				endfo
			*else
			*	messagebox("Codigo de CUIT no valido",48,"Control")	
			*	this.value=""
			*	node
			*endif	
		endif	
		
	ENDPROC

	PROCEDURE Edit1.LostFocus
		*thisform.edit1.visible=.f.	
		*thisform.shape3.visible=.f.
	ENDPROC

	PROCEDURE Edit1.RightClick
		*DO edicion.mpr WITH THIS
	ENDPROC

	PROCEDURE Grid1.Column1.Text1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
				do form busqueda with 'bancos',allt(this.value)+chr(nkeycode),"Codigo","Nombre";
				,"Tipo_Banco","=","N","Codigo","Nombre" to this.value
		EndIf
	ENDPROC

	PROCEDURE Grid1.Column1.Text1.LostFocus
		If val(this.value)#0
			this.Value=ceros(this.Value,3)
			Select bancos
			Set Filter To 
			Set Order To codigo
			Seek this.Value
			If Found()
				If Tipo_Banco="N"
					Select aux_Porcentaje_Proveedores
					replace Nombre with bancos.nombre
				else
					MessageBox("Esta es una Cuenta Bancaria",48,"Control")	
					this.Value=""
					node
				Endif	
			Else
				Select aux_Porcentaje_Proveedores
				replace nombre with ""
				MessageBox("Codigo de Cuenta Incorrecto",48,"Control")
