	PROCEDURE Xpbutton2.Click
		if val(thisform.codigo1.value)=0
			messagebox("Debe Cargar Codigo de Proveedor",48,"Control")
			thisform.codigo1.setfocus
			return
		endif
		if val(thisform.cuit1.value)#0
			for a =1 to thisform.totreg
				if thisform.cuit1.value=cuit(a) and thisform.codigo1.value#cli(a)
					if messagebox('Este Nro. de C.U.I.T. ya Existe.'+chr(13)+"¿Desea Seguir?",36,'Seguridad')=7
						thisform.cuit1.setfocus
						return
					Endif	
				endif
			endfor
		else
			messagebox("Debe Cargar el CUIT de su proveedor",48,"Control")	
			thisform.cuit1.setfocus
			return	
		endif
		if messagebox('Confirma grabar proveedor',36,'Grabar')=6
			sele proveedores
			set order to codigo
			seek thisform.codigo1.value
			=tableupdate(.t.)
			appe blan
			******* Grabo Porcentaje_Proveedores *******
			Select aux_Porcentaje_Proveedores
			Go top
			DO while not Eof() and Val(cuenta)#0
				Scatter memvar
				m.proveedor=thisform.codIGO1.Value
				Select Porcentaje_Proveedores		
				If m.registro=0
					If m.porcentaje#0
						Append Blank
						Gather memvar
					EndIf	
				Else
					Go Record m.registro
					If m.porcentaje=0
						Delete
					Else
						Gather memvar	
					Endif		
				EndIf	
				Select aux_Porcentaje_Proveedores
				Skip
			EndDo
			Select aux_Porcentaje_Proveedores
			=TableRevert(.t.)
		endif	
		thisform.totreg=0
		select proveedores
		set order to cuit
		go top
		scan
			thisform.totreg=thisform.totreg+1
		endscan
		dimen cuit(thisform.totreg)
		dimen cli(thisform.totreg)
		select proveedores
		set order to cuit
		go top
		for a =1 to thisform.totreg
			cuit(a)=proveedores.cuit
			cli(a)=proveedores.codigo
			skip 
		endfo
		thisform.label1.caption=""
		thisform.codigo1.value=space(6)
		select proveedores
		set order to codigo
		go bott
		thisform.labelultimo.caption=codigo
		skip
		thisform.refresh
		thisform.codigo1.setfocus
		
	ENDPROC

	PROCEDURE Xpbutton3.Click
		if 	messagebox('Confirma eliminar proveedor ',36,'Eliminar ')=6
			Select Porcentaje_Proveedores	
			Set Order To Proveedor
			Seek thisform.coDIGO1.Value
			DO while not Eof() and proveedor=thisform.codiGO1.Value
				dele
				Select porcentaje_Proveedores
				Skip
			EndDo
			Select Aux_Porcentaje_Proveedores	
			=TableRevert(.t.)
			sele proveedores
			seek thisform.codigo1.value
			dele
			=tableupdate(.t.)
			appe blan
		endif
		thisform.totreg=0
		select proveedores
		set order to cuit
		go top
		scan
			thisform.totreg=thisform.totreg+1
		endscan
		dimen cuit(thisform.totreg)
		dimen cli(thisform.totreg)
		select proveedores
		set order to cuit
		go top
		for a =1 to thisform.totreg
			cuit(a)=proveedores.cuit
			cli(a)=proveedores.codigo
			skip 
		endfo	
		thisform.label1.caption=""
		thisform.codigo1.value=space(6)
		select proveedores
		set order to codigo
		go bott
		thisform.labelultimo.caption=codigo
		skip
		thisform.refresh
		thisform.codigo1.setfocus
	ENDPROC

	PROCEDURE Xpbutton4.Click
		thisform.p=thisform.p+1
		if thisform.p=3
			thisform.p=1
		endif
		if thisform.p=1
			thisform.edit1.visible=.t.
			thisform.shape3.visible=.t.
			thisform.edit1.setfocus
		else
			thisform.edit1.visible=.f.	
			thisform.shape3.visible=.f.
		endif
	ENDPROC

