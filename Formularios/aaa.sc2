			this.value=ceros(this.value,3)
			select tipo_retencio
			set orde to codigo
			seek this.value
			if found()
			   repl proveedores.codali with codigo
			   repl proveedores.nomali with nombre
			   repl proveedores.ali with alicuota
			ELSE
		        repl proveedores.nomali with ""
		        repl proveedores.ali with 0
				this.value=""
				messagebox("Código de Alicuotas Incorrecto",48,"Control")
			endif
		else	
		   repl proveedores.codali with ""
		   repl proveedores.nomali with ""
		   repl proveedores.ali with 0
		ENDIF
		
		thisform.Refresh 
		
