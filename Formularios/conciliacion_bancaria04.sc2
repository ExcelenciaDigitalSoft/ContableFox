	PROCEDURE Command5.Click
		If thisform.banco1.Value='003'
			_nomtabla='coseguro003'
		Else
			_nomtabla='coseguro002'
		EndIf
		MESSAGEBOX(_nomtabla)
		Select aux_movbanco
		Scan
			Scatter memvar
			if val(m.nrocupon)>0
				Select &_nomtabla
				Set Order To numero
				Seek m.nrocupon
				If Found()
					repl aux_movbanco.obs with ' -- PENDIENTE --'&& esta bien, dejarlo pendiente
				Else
					sele aux_movbanco
					repl aux_movbanco.obs with 'Debería estar conciliado.'		&& debería estar conciliado
				EndIf
			endif
		endscan
		sele aux_movbanco
		go top
		brow field nrocupon,obs
		excel('aux_movbanco')
		
		REPL ALL OBS WITH ''
		
		Select &_nomtabla
		Scan
			Scatter memvar
			Select aux_movbanco
			locate for nrocupon=ALLT(m.numero)
			If Found()
				Select &_nomtabla
				REPL OBSERV with 'existe'&& esta bien, dejarlo pendiente
			Else
				Select &_nomtabla
				REPL OBSERV  with 'NO EXISTE'		&& debería estar conciliado
			EndIf
			
		endscan
		sele &_NOMTABLA
		go top
		brow field nUMERO,OBSERV
	ENDPROC

