	PROCEDURE Command3.Click
		
		IF val(THISFORM.Text4.VALUE)=0
			MESSAGEBOX("Debe ingresar un Numero de Conciliación",48,"Control")
			thisform.Text4.setfocus
			return
		ENDIF
		
		IF THISFORM.TXTimporte.VALUE=0
			MESSAGEBOX("Debe ingresar un Importe",48,"Control")
			thisform.txtimporte.setfocus
			return
		ENDIF
		
		IF LEN(ALLT(THISFORM.TXTDETALLE.VALUE))=0
			MESSAGEBOX("Debe ingresar un Detalle",48,"Control")
