		   	endif		
			thisform.grid2.SetAll("dynamicbackcolor","IIF(copiado=1,RGB(255,223,191),RGB(255,255,255))","Column")
			thisform.grid3.refresh
			select aux_faccom
			reg=recno()
			go record reg
