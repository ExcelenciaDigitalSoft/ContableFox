			    *if thisform.efec_ret=.t.
			    if .t.
			    
					
				    *************************
				    sele aux_perlotee 
			        tablerevert(.t.)
			        
				    sele aux_perlote
					go top
					do while not eof()
					   scatter memvar
					   sele aux_perlotee
					   appen blank
		   			   gather memvar
					Sele aux_perlote
					skip
					enddo 
				    *************************
				    sele aux_perlote 
			        tablerevert(.t.)
			        
			        	        
			        sele aux_perlotee 
				    set order to factura
				    go top
				    do while not eof()
				       if factura<>aux_faccom.numero  
				         scatter memvar 
				         sele aux_perlote 
				         appen blank
				         *percepcion=0
					     *lotehogar=0
					     *total=0
					     *codali=""
					     *alicuota=0
				         gather memvar
				       endif   
				    sele aux_perlotee 
				    skip
				    enddo       
				    
			        sele aux_perlotee 
			        tablerevert(.t.)
			        
			    
			   		sele aux_perlote
					go top
					do while not eof()
					   scatter memvar
					   sele aux_perlotee
					   appen blank
					   *percepcion=0
		        	   *lotehogar=0
					   *total=0
					   *codali=""
					   *alicuota=0
		    		   gather memvar
					Sele aux_perlote
					skip
					enddo 
					
					thisform.suma3()
				
			     	thisform.grid4.refresh
			        set order to 
			     	thisform.grid1.refresh
			    else
		   			thisform.grid4.column1.readonly=.t.
					thisform.grid4.column7.readonly=.t.
					thisform.grid4.column2.readonly=.t.
					thisform.grid4.column3.readonly=.t.
					thisform.grid4.column4.readonly=.t.
		            thisform.grid4.column5.readonly=.t.
					thisform.grid4.refresh 
		 	
		     	endif 
