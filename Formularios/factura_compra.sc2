	PROCEDURE FECHA1.LostFocus
		if Betw(this.value,_periododesde,_periodohasta)
			sele faccom
			set orde to numero
			seek thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
			if !found()	
				******* Controlo Fecha de Factura Compra *******
				if !Betw(this.value,(date()-thisform.aviso_fec_compra),date())
					if messagebox("Esta fecha es correcta",36,"Control")=7
						this.value=date()
					endif
				endif	
				******* Calculo Fecha de Vencimiento *******			
				store 0 to inicio	
				select proveedores
				set order to codigo
				seek thisform.cliente1.value
				if found()
					d_vence=proveedores.dias_vence
					select dias
					set order to nombre
					seek proveedores.dia_cierre
					if found()
						if dias.codigo#0
							if dow(this.value)=dias.codigo
								inicio=0
							else
								if dow(this.value)>dias.codigo	
									inicio=(7+dias.codigo)-dow(this.value)
								else
									inicio=dias.codigo-dow(this.value)
								endif
							endif
						else
							inicio=0	
						Endif	
					else
						inicio=0
					endif
				endif
				select aux_venci
				go top
				repl dias with inicio+d_vence
				repl fecven with thisform.fecha1.value+dias
				repl factura with thisform.letra+thisform.num1.value+thisform.numero1.value
				repl proveedor with thisform.cliente1.value		
				thisform.grid1.refresh
			endif
		else
			messagebox("Esta Cargando un Fecha que no pertenece al Ejercicio Contable Seleccionado",48,"Control")	
			this.value=date()
			node
		Endif	
	ENDPROC

	PROCEDURE Fecha_contable.LostFocus
		if Betw(this.value,_periododesde,_periodohasta)
			if thisform.mesperiodo.value=space(2) and thisform.añoperiodo.value=space(4)
				thisform.mesperiodo.value=ceros(str(month(thisform.fecha_contable.value),2),2)
				thisform.añoperiodo.value=str(year(thisform.fecha_contable.value),4)
			endif
		else
			messagebox("Esta Cargando un Fecha que no pertenece al Ejercicio Contable Seleccionado",48,"Control")	
			this.value=date()
			thisform.mesperiodo.value=ceros(str(month(thisform.fecha_contable.value),2),2)
			thisform.añoperiodo.value=str(year(thisform.fecha_contable.value),4)	
		Endif	
	ENDPROC

