	PROCEDURE asiento
		param campo,tip &&//El campo es la clave que hace referencia a los asientos.Los tipos posibles son : (sin datos es para limpiar),mostrar,grabar,eliminar
		store space(6) to asiento
		store space(20) to cam
		cam=alltr(campo)
		with thisform
			select detacont
			set filte to periodo=_periodosele &&periodo_actual.periodo
			select movicont 
			set filte to periodo=_periodosele  &&periodo_actual.periodo
			if empty(cam) and empty(tip)
				.asiento1.text4.value=0.00	
				.asiento1.text5.value=0.00	
				.asiento1.text6.value=0.00	
				thisform.asiento1.nro.caption=""
				select tabla15
				=tablerevert(.t.)
				append blank
				.asiento1.refresh
			else	
				.asiento1.text4.value=0.00	
				.asiento1.text5.value=0.00	
				.asiento1.text6.value=0.00	
				do case
					case alltr(tip)='mostrar'	
					
					select detacont
					Set Filter To 	
					select movicont 
					set filte to 
					
						select tabla15
						=tablerevert(.t.)			
						select movicont
						set order to faccom
						seek alltr(cam)
						if found()
							thisform.asiento1.nro.caption="Asiento N°: "+alltr(movicont.nroasiento)
							do while not eof() and faccom=alltr(cam)
								scatte memvar
								m.registro=recno()
								sele cuencont
								set order to codigo
								seek m.codigo
								m.detalle=nombre
								sele tabla15
								appen blan
								if m.debehaber='D'
									m.debe=importe
									m.haber=0
									thisform.asiento1.text4.value=thisform.asiento1.text4.value+importe
									thisform.asiento1.text5.value=thisform.asiento1.text5.value+importe
								else
									m.haber=importe
									m.debe=0
									thisform.asiento1.text4.value=thisform.asiento1.text4.value-importe 
									thisform.asiento1.text6.value=thisform.asiento1.text6.value+importe
								endif
								gather memvar
								sele movicont
								skip
							enddo
						endif
					case alltr(tip)='grabar'	
						select detacont
						
						set order to faccom
						seek cam
						if found()
							asiento=nroasiento
							uni=unico
						else
							set filter to periodo=_periodosele
							set order to nroasiento
							go bott
							asiento=ceros(str(val(nroasiento)+1,6),6)	
							uni=sys(2015)
							append blank
						endif
						repl nroasiento with asiento
						repl periodo with _periodosele  &&periodo_actual.periodo
						repl unico with uni
						repl detalle with "COMPRA N°:"+Left(Alltrim(cam),14)&&cam+' '+alltr(thisform.nombre1.value)
						Repl leyenda with Upper(alltr(thisform.nombre1.value))
						repl fecha    with thisform.fecha_contable.value					
						repl faccom with alltr(cam)
						sele tabla15
						go top
						do while not eof() and val(codigo)#0
							scatter memvar
							letra=iif(m.debe#0,'D','H')
							m.unico=uni
							m.nroasiento=asiento
							m.faccom=alltr(cam)
							m.codigo=m.codigo
							m.fecha=thisform.fecha_contable.value
							m.recibo=space(6)
							m.factura=space(14)
							m.o_ingreso=space(14)
							m.chequeter=space(6)
							m.periodo= _periodosele &&periodo_actual.periodo
							if letra='D'
								m.debehaber='D'
								m.importe=m.debe
							else
								m.debehaber='H'
								m.importe=m.haber
							endif	
							sele movicont
							if m.registro=0
								If (m.debe+m.haber)#0
									appe blan
									gather memvar
								EndIf	
							else
								go record m.registro
								if m.debe=0	and m.haber=0
									dele 
								else
									gather memvar	
								endif
							endif
							sele tabla15
							skip
						enddo
					case alltr(tip)='eliminar'	
						select detacont
						Set Order To 
						set order to faccom
						seek cam
						if found()
							dele
						endif
						select movicont
						Set Order To 
						set order to faccom
						seek cam
						if found()
							do while not eof() and faccom=alltr(cam)
								dele
								seek cam
							enddo
						endif					
				endcase
				.asiento1.grid1.refresh	
			endif	
			select detacont
			set filte to 
			select movicont
			set filte to
		endwith	
	ENDPROC

	PROCEDURE asi_automatico
		select tabla15
		=tablerevert(.t.)
		******* Mercaderia *******
		if thisform.total1.value-(thisform.retper1.value+thisform.tot_iva.value)#0
			select cuencont
			set order to codigo
			seek thisform.cuenta_mercaderia
			if found()
				select tabla15
				set order to codigo
				seek thisform.cuenta_mercaderia
				if !found()
					append blank
				endif
				if left(alltr(thisform.letra),1)#'C'	
					repl codigo with thisform.cuenta_mercaderia,detalle with cuencont.nombre
					repl debe with thisform.total1.value-(thisform.retper1.value+thisform.tot_iva.value),haber with 0.00		
				else
					repl codigo with thisform.cuenta_mercaderia,detalle with cuencont.nombre
					repl HABER with thisform.total1.value-(thisform.retper1.value+thisform.tot_iva.value),DEBE with 0.00
				endif	
			endif	
		endif
		*//Iva Credito Fiscal
		if thisform.tot_iva.value#0
			select cuencont
			set order to codigo
			seek thisform.iva
			if found()
				select tabla15
				set order to codigo
				seek thisform.iva
				if !found()
					append blank
				endif
				if left(alltr(thisform.letra),1)#'C'	
					repl codigo with thisform.IVA,detalle with cuencont.nombre
					repl debe with thisform.tot_iva.value,haber with 0.00		
				else
					repl codigo with thisform.IVA,detalle with cuencont.nombre
					repl HABER with thisform.tot_iva.value,DEBE with 0.00
				endif	
			endif	
		endif
		*//Retencion y Persepcion
		if thisform.RetPer1.value#0
			select cuencont
			set order to codigo
			seek thisform.cuenta_retencion
			if found()
				select tabla15
				set order to codigo
				seek thisform.cuenta_retencion
				if !found()
					append blank
				endif
				if left(alltr(thisform.letra),1)#'C'	
					repl codigo with thisform.cuenta_retencion,detalle with cuencont.nombre
					repl debe with thisform.RetPer1.value,haber with 0.00		
				else
					repl codigo with thisform.cuenta_retencion,detalle with cuencont.nombre
					repl HABER with thisform.Retper1.value,DEBE with 0.00
				endif	
			endif	
		endif
		*// a Proveedor 
		select proveedores
		set order to codigo
		seek thisform.cliente1.value
		if found()
			select cuencont
			set order to codigo
			seek proveedores.cuenta
			if found()
				select tabla15
				set order to codigo
				seek proveedores.cuenta
				if !found()
					appen blank
				endif
				if left(alltr(thisform.letra),1)#'C'	
					repl codigo with proveedores.cuenta,detalle with cuencont.nombre
					repl debe with 0.00,haber with thisform.total1.value
				else
					repl codigo with proveedores.cuenta,detalle with cuencont.nombre
					repl HABER with 0.00,DEBE with thisform.total1.value
				endif	
			endif
		endif		
		select tabla15
		thisform.asiento1.text4.value=0.00
		thisform.asiento1.text5.value=0.00
		thisform.asiento1.text6.value=0.00
		go top
		scan
			thisform.asiento1.text5.value=thisform.asiento1.text5.value+debe
			thisform.asiento1.text6.value=thisform.asiento1.text6.value+haber
		endscan
		thisform.asiento1.text4.value=thisform.asiento1.text5.value-thisform.asiento1.text6.value
		select tabla15
		set order to 0
		thisform.asiento1.refresh
		
	ENDPROC

