	PROCEDURE Xpbutton3.Click
		if messagebox('Confirma eliminar factura',36,'Eliminar')=6
			sele faccom
			set order to numero
			seek thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
			if found()
				dele
			endif
			******* Elimino Asientos *******
			thisform.asiento(thisform.letra+thisform.num1.value+thisform.numero1.value+thisform.cliente1.value,'eliminar')
			select tabla15
			=tablerevert(.t.)
			thisform.asiento1.grid1.refresh	
			******* Elimino Iva Compras *******
			select Iva_FacCom
			set orde to factura
			seek thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
			do while not eof() and proveedor=thisform.cliente1.value and factura=thisform.letra+thisform.num1.value+thisform.numero1.value
				dele
				seek thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
			EndDo
			******* Elimino los Vencimientos Cargados *******
			select venci_compra
			set orde to factura
			seek thisform.letra+thisform.num1.value+thisform.numero1.value+thisform.cliente1.value
			do while not eof() and factura=thisform.letra+thisform.num1.value+thisform.numero1.value and proveedor=thisform.Cliente1.value
				dele
				seek thisform.letra+thisform.num1.value+thisform.numero1.value+thisform.cliente1.value
			Enddo
			******* Elimino Movimientos Bancos *******
			select movimientos_bancos
			set order to faccom
			seek thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
			do while not eof() and faccom=thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
				dele
				seek thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
