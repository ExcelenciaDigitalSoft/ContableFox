	PROCEDURE suma
		with thisform
			.tot_neto.value=0.00
			.tot_iva.value=0.00
			.tot_exento.value=0.00
			.tot_gravado.value=0.00
		select aux_IvaFaccom
		_r=recno()
		go top
		scan
			.tot_neto.value=.tot_neto.value+neto
			.tot_iva.value=.tot_iva.value+iva
			.tot_exento.value=.tot_exento.value+exento
			.tot_gravado.value=.tot_gravado.value+grabado
		endscan
		go record _r
			******* Totales *******
			.total1.value=.tot_exento.value+.tot_gravado.value+thisform.retper1.value
			******* Condiciones de Compra *******
			.ctacte1.value=.total1.value-(.efectivo1.value+.text1.value+.text10.value)
		endwith
	ENDPROC

