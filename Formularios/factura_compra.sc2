			Seek thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
			DO while !Eof() and costos=thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
				Delete
				Select movimientos_bancos
				skip
			EndDo
			select movimientos_bancos
			=tableupdate(.t.)
			******* Elimono Cheques de Terceros *******
			select chequeter
			set order to faccom
			seek thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
			do while not eof() and faccom=thisform.cliente1.value+thisform.letra+thisform.num1.value+thisform.numero1.value
				repl fechadep with {}
				repl faccom with ""
				select chequeter
				skip
			enddo
			thisform.cartera1.grid1.refresh			
		endif
		******* Inicializo Formulario *******
