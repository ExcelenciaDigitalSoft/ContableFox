		endif
		
	ENDPROC

	PROCEDURE Text3.LostFocus
		this.backcolor=c_salida
		if val(this.value)#0
			this.value=ceros2(this.value,6)
			select cuencont
			set order to codigo
			seek this.value
			if !found()
				=messagebox('Codigo de Cuenta Incorrecto',48,'Control')
				this.value=space(6)
				node
			endif
		endif
	ENDPROC

