	ENDPROC

	PROCEDURE Xpbutton2.Click
		if messagebox("Confirma Grabar Datos",36,"Grabar")=6
			select bancos
			set order to codigo
			seek thisform.text1.value
			if !found() and thisform.text1.value # SPACE(2)
			   	append blan
			endif	  
			repl codigo with thisform.text1.value
			repl nombre with thisform.text2.value
			repl cuenta with thisform.text3.value
			repl sal_inicio with thisform.text4.value
			repl fecha with thisform.text5.value
			do case
				case thisform.o.o1.value=1
					repl tipo_banco   with "N"
					repl modi_cartera with "N"
				case thisform.o.o2.value=1
					repl tipo_banco   with "S"
					repl modi_cartera with "N"
				case thisform.o.o3.value=1
					repl tipo_banco   with "N"
					repl modi_cartera with "S"
			endcase
			= tableupdate(.t.)
			thisform.text1.value= space(2)
			thisform.text2.value=space(30)
			thisform.text3.value=space(30)
			thisform.text4.value=0.00
			thisform.text5.value={}
			thisform.grid1.refresh
			thisform.text1.setfocus
		endif	
		
	ENDPROC

	PROCEDURE Xpbutton3.Click
		if 	messagebox('Confirma Eliminar Banco',36,'Eliminar ')=6
			sele bancos
			dele
			=tableupdate(.t.)
			thisform.text1.value = space(2)
