	PROCEDURE Text1.GotFocus
		for k=1 to 3
			keyboard'{SHIFT+RIGHTARROW}'
		endfor
		this.backcolor=c_entrada
	ENDPROC

	PROCEDURE Text1.LostFocus
		this.backcolor=c_salida
		if val(this.value)#0
			this.value=ceros(this.value,3)
			select bancos
			set order to codigo
			seek this.value
			if found()
				thisform.text2.value = nombre
				thisform.text3.value = cuenta
				thisform.text4.value = sal_inicio
				thisform.text5.value = fecha
				do case
					case tipo_banco="N" and modi_cartera="N"
						thisform.o.o1.value=1
						thisform.o.o2.value=0
						thisform.o.o3.value=0
					case tipo_banco="S" and modi_cartera="N"
						thisform.o.o1.value=0
						thisform.o.o2.value=1
						thisform.o.o3.value=0
					case tipo_banco="N" and modi_cartera="S"
						thisform.o.o1.value=0
						thisform.o.o2.value=0
						thisform.o.o3.value=1
				endcase		
			else
				thisform.text2.value = space(30)
				thisform.text3.value = space(30)
				thisform.text4.value = 0.00
				thisform.text5.value = date()
				thisform.o.o1.value=1
				thisform.o.o2.value=0
				thisform.o.o3.value=0		
			endif		
		endif
		
	ENDPROC

