	PROCEDURE ceros
		parameter x,y
		x=allt(x)
		x=repl('0',y-len(x))+x
		return x
	ENDPROC

	PROCEDURE ceros2
		parameter x
		return allt(x)+repl('0',6-len(allt(x)))
		
	ENDPROC

	PROCEDURE Click
		thisform.grid2.visible=.F.
		
	ENDPROC

	PROCEDURE Init
		thisform.grabado=.f.
		
		
		select periodo_actual
		go top
		if cerrado="S"
			thisform.xpButton2.visible=.f.
			thisform.xpButton3.visible=.f.
		Endif
		select detacont
		set filte to periodo=periodo_actual.periodo
		select movicont 
		set filte to periodo=periodo_actual.periodo
		thisform.fecha1.value=date()
		thisform.nroasiento1.value=space(6)
		thisform.text1.value=space(50)
		thisform.text2.value=space(6)
		thisform.text3.value=space(3)
		thisform.text4.value=0.00
		sele detacont
		go bottom
		thisform.text2.value=nroasiento
		thisform.nroasiento1.value=Ceros(Str(Val(nroasiento)+1,6),6)
		Thisform.nroASIENTO1.Visible= .F.
		Thisform.xpbutton2.Visible = .F.
		Thisform.xpbutton3.Visible = .F.
		Thisform.xpbutton4.Visible = .F.
		Thisform.xpbutton5.Visible = .F.
	ENDPROC

	PROCEDURE Command1.Click
		if thisform.text5.value+thisform.text6.value=0
			select aux_detacont
			=tablerevert(.t.)
			select * from detacont where tipo_asi="002" ;
			order by periodo ;
			into cursor consulta
			select consulta
			go top
			scan
				repl periodo with  ''
				select consulta
			endscan
		else
			messagebox("Elimine los Asientos y luego Importe",48,"Control")
		endif
		
	ENDPROC

	PROCEDURE FECHA1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		
	ENDPROC

	PROCEDURE FECHA1.LostFocus
		if !Betw(this.value,periodo_actual.desde,periodo_actual.hasta)
			messagebox("Esta Cargando una Fecha fuera del Ejercicio Contable Seleccionado",48,"Control")
			this.value=date()
		endif
		
	ENDPROC

	PROCEDURE Grid1.Column1.Text1.GotFocus
		select tabla15
		thisform.text4.value=0.00
		thisform.text5.value=0.00
		thisform.text6.value=0.00
		reg=recno()
		go top
		scan
			thisform.text5.value=thisform.text5.value+debe
			thisform.text6.value=thisform.text6.value+haber
		endscan
		thisform.text4.value=thisform.text5.value-thisform.text6.value
		go record reg
		thisform.grid1.refresh
	ENDPROC

	PROCEDURE Grid1.Column1.Text1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if nkeycode=13 and this.value=space(6)
			thisform.Xpbutton2.setfocus
		endif
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
			do form busqueda with 'cuencont','','Codigo','Nombre' to this.value
		endif
		
	ENDPROC

	PROCEDURE Grid1.Column1.Text1.LostFocus
		if val(this.value)#0
			this.value=ceros2(this.value,6)
			sele cuencont
			set order to codigo
			seek this.value
			if found()
				repl tabla15.detalle with nombre
			else
				=messagebox('Cuenta No Encontrada.',48,'Control')
			endif
		endif
		
	ENDPROC

