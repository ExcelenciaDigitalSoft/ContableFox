			EndIf
		Else
		
		
		
		EndIf
		
	ENDPROC

	PROCEDURE Init
		
		
		*------------------ Buscar Cuenta de Ajuste --------------------------------------------------------
		Select cuencont
		Set Filter To ctaajuste=1
		Count to cuenajus
		If cuenajus=0
			MessageBox("No está configurada la Cuenta de Ajuste",48,"Control")
		Else
			If cuenajus>1
				MessageBox("Hay mas de una Cuenta de Ajuste Definida, se tomará la primera",48,"Control")
				Go top
			Else
				Go top
			EndIf
			thisform.txtcuenta_ajuste.Value=codigo
			thisform.txtnom_cuentaajuste.Value=Alltrim(nombre)
		EndIf
		
		Set Filter To 
		*-----------------------------------------------------------------------------------------------------
		
		thisform.txtperiodo.Value=Alltrim(Str(Year(Date())-1))+'01'
		
		thisform.grdAux_ajusdetalle.SetAll("dynamicbackcolor","IIF(error=1,rgb(255,159,159),iif(asi_inicio=1,rgb(177,208,224),iif(.f.,RGB(177,208,224),RGB(255,255,255))))","Column")	
		
		thisform.grdAux_ajusasiento.SetAll("dynamicbackcolor","IIF(ctaajuste=1,rgb(0,255,128),RGB(255,255,255))","Column")	
		
		Store 0 to thisform.txttotal_debeasiento.Value,thisform.txttotal_debedetalle.Value,thisform.txttotal_haberasiento.Value,thisform.txttotal_haberdetalle.Value,thisform.txtdiferencia.value
		
		*---- CONTROLES ---------------------------------------------------------------------------------------
		*Use movicont
		*Set filter to periodo<>Alltrim(Str(Year(fecha)))+'01' and Between(fecha,{01/01/2018},{31/12/2018})
		
		*Select * from movicont m where Between(fecha,{01/01/2018},{31/12/2018}) and not exists (select * from detacont d where m.unico=d.unico)
		
		*Select * from movicont m where Between(fecha,{01/01/2018},{31/12/2018}) and not exists (select * from detacont d where m.periodo=d.periodo and m.nroasiento=d.nroasiento)
		*------------------------------------------------------------------------------------------------------
	ENDPROC

	PROCEDURE ver_ajuste
		Parameters _idcierre
		
		Select aux_ajusdetalle
		TableRevert(.t.)
		
		Select aux_ajusasiento
		TableRevert(.t.)
		
		
		
		thisform.lblFECHA1.Visible=.f.
		thisform.txtfecha_asiento.Visible=.f.
		thisform.txtdetalle_asiento.Visible=.f.
		thisform.labeldetalle.Visible= .f.
		thisform.b_grabar.Visible=.f.
			
		Store 0 to thisform.txttotal_debeasiento.Value,thisform.txttotal_debedetalle.Value,thisform.txttotal_haberasiento.Value,thisform.txttotal_haberdetalle.Value,thisform.txtdiferencia.value
		
		thisform.lab_errores.Visible=.f.
		
		thisform.b_anular.Visible=.f.
				
		_errordet=''
		_error=0
		_errorgral=0
		
		If _idcierre=0
			
			_len=Len(Alltrim(thisform.txtcuenta_filtro.Value))
			_cond=Iif(_len=0,'.t.','left(codigo,_len)=Alltrim(thisform.txtcuenta_filtro.Value)')
		
			Select * from cuencont where ajusteinf=1 and &_cond into cursor CuentasAjustables
		
			Select CuentasAjustables
			If Reccount()=0
				MessageBox("No hay Cuentas Marcadas para Ajustar",64,"Aviso")
				Return
			endif
		
		
			_fechadesde=Ctod('01/'+Right(thisform.txtperiodo.Value,2)+'/'+Left(thisform.txtperiodo.Value,4))
			_fechahasta=thisform.txtfechacierre.Value
			
			*_fechadesdeapertura=Ctod('01/'+Right(thisform.txtperiodo_indaper.Value,2)+'/'+Left(thisform.txtperiodo_indaper.Value,4))
			_perdesdeapertura=thisform.txtperiodo_indaper.Value  && Para el asiento de 
			
			_MovAjustados=0
				
			Select CuentasAjustables
			Scan
				_cuenta=codigo
				
				Select movicont
				Set Order To CODIGOFECH   && CODIGO+DTOS(FECHA)
				Set Near on
				Seek _cuenta+Dtos(_fechadesde)
				Set Near off
				DO while not Eof() and codigo=_cuenta
					
					_errordet=''
					_error=0
		
					Scatter memvar
					_recno=Recno()
					
					If m.unico='_53L11EBFP'
		*				a==
					endif
					
					If Between(fecha,_fechadesde,_fechahasta) && and ajusinf=1
						_encontro_cab=.t.
						_leyenda=''
						
						Select detacont
						Set Order To unico
						Seek m.unico
						If Found()
						Else
							Set Order To perasiento
							Seek m.periodo+m.nroasiento
							If Found()
								
							Else
								_encontro_cab=.f.	
							EndIf
						Endif
						
						_procesa=.t.
						Store .f. to _asiento_apertura,_asiento_cierre
						
						If _encontro_cab
						
							*---- Si el asiento está marcado como NO AJUSTAR el ajuste aunque la cuenta aplique no lo tomo.
							If noajustar=1 
								_procesa=.f.
							endif
						
							Select detacont
							If tipo_asi='001'  && Apertura
								_asiento_apertura=.t.
								*-- Si está tildado No aplicar a movimientos de bienes de uso de asiento de apertura, no aplico para AMortización Si.
								If CuentasAjustables.bienesuso=1 and thisform.check_noaplicaajusteabienesdeusoenapertura.Value=1 
									_procesa=.f.  && Si es cuenta de Bienes de Uso no aplico ajuste a la cuenta de Bienes de Uso, si a las de Amortización.
								Else
									
								EndIf
								
								*-- Si está tildado No aplicar a movimientos de Amortización de asiento de apertura, no aplico para AMortización Si.
								If CuentasAjustables.amortizacion=1 and thisform.check_noaplicaajusteamortizacionenapertura.Value=1 
									_procesa=.f.  && Si es cuenta de Bienes de Uso no aplico ajuste a la cuenta de Bienes de Uso, si a las de Amortización.
								Else
									
								EndIf
								
								
							EndIf
							
							If tipo_asi='002'  && Cierre del Ejercicio
								If thisform.check_noaplicaajusteenasientoCierre.Value=1
									_procesa=.f.  && Que hago con el Asiento de Cierre??
								endif
								_asiento_cierre=.t.
							EndIf
							_leyenda=Alltrim(Detalle) + ' '+ Alltrim(leyenda)
						Else
							*MessageBox("Cabecera de Asiento no encontrada: "+movicont.nroasiento)
						endif	
						
						If _procesa
						
							_MovAjustados=	_MovAjustados + 1
						
							Select aux_ajusdetalle
							Append Blank
							Gather memvar
												
							Replace leyenda with _leyenda
							Replace impbase 		with importe
							
							_percierre=Alltrim(Str(year(thisform.txtfechacierre.Value))+ceros(Str(Month(thisform.txtfechacierre.Value)),2))
							_perinicio=Alltrim(Str(year(m.fecha))+ceros(Str(Month(m.fecha)),2))
							
							If _asiento_apertura
								Replace asi_inicio		with 1
								_perinicio=thisform.txtperiodo_indaper.Value
							EndIf
							
							_indinicio=0
							_indcierre=0
							_coeficient=1
							
							Select indice_infla
							Set Order To periodo
							Seek _perinicio
							If Found()
								_indinicio=indice
							else
								_error=1
								_errordet=_errordet+' Indice Periodo: '+_perinicio+' No encontrado.'
							endif
							Seek _percierre
							If Found()
								_indcierre=indice
							Else
								_error=1
								_errordet=_errordet+' Indice Periodo: '+_percierre+' No encontrado.'
							EndIf
							If _indinicio<>0
								_coeficient=_indcierre	/ _indinicio
							Else
								_error=1
								_errordet=_errordet+' Indice Periodo: '+_perinicio+' es 0, no se puede dividir por 0 para sacar el coeficiente'
							EndIf
							
							Select aux_ajusdetalle
							Replace asientoori		with m.periodo+m.nroasiento
							Replace unicoori			with m.unico
							Replace indinicio 		with _indinicio
							Replace indcierre		with _indcierre
							Replace coeficient		with _coeficient
							Replace perinicio		with _perinicio
							Replace percierre		with _percierre
							Replace importe 		with impbase * _coeficient
							Replace impajuste 		with importe - impbase
							Replace registro 		with _recno
							
							If debehaber='D'
								Replace debe 		with impajuste
							endif
							If debehaber='H'
								Replace haber 		with impajuste
							endif
							 
							Select cuencont
							Set Order To codigo
							Seek m.codigo
							If Found() 
								Select aux_ajusdetalle
								Replace nomcuenta with Alltrim(cuencont.nombre)
							EndIf
							
							If _error<>0
								Replace error with 1
								Replace errordet with _errordet
								
								_errorgral=_errorgral + 1
							endif
						EndIf
						
					endif
		
					Select movicont
					SKIP
				EndDo
				
				
			EndScan
			
			*--- AGrupo por Cuenta. ---------------------------------------------------
			Select aux_ajusdetalle
			If _MovAjustados>0
				Select aux_ajusdetalle
				Scan
					Scatter memvar
					
					If impajuste>0
						Select aux_ajusasiento
						Set Order To codigo
						Seek m.codigo
						If not Found()
							Append Blank
							Gather memvar
							Replace leyenda with ''
							*Replace detalle with ''
							
						Else
							Replace importe 		with importe + m.importe
							Replace impajuste 		with impajuste + m.impajuste
							Replace impbase 		with impbase + m.impbase
							Replace debe			with debe + m.debe
							Replace haber 			with haber + m.haber
						endif
					EndIf
					
				EndScan
				
				
				*---- Ajusto los saldos al Debe o al Haber. --------------------------------
				Select aux_ajusasiento
				Scan
					If debe<>0 and haber<>0
						If debe >= haber
							Replace debe with debe - haber
							Replace haber with 0
							Replace debehaber with 'D'
						Else
							Replace haber with haber - debe 
							Replace debe with 0
							Replace debehaber with 'H'					
						endif
					endif	
					If debe<>0
						Replace importe with debe
					EndIf
					If haber<>0
						Replace importe with haber
					endif
				EndScan
				*-----------------------------------------------------------------------------
				
				*-------  Agrego la cuenta de AJuste con el saldo resultante. ---------------------------------------
				Select aux_ajusasiento
				Sum debe to _totdebe
				Sum haber to _tothaber
				_difAjuste=0
				Append Blank
				Replace codigo with thisform.txtcuenta_ajuste.Value
				Replace nomcuenta with thisform.txtnom_cuentaajuste.value
				
				If _totdebe < _tothaber
					Replace importe with _tothaber - _totdebe
					Replace debehaber with 'D'
					Replace debe with importe
					Replace haber with 0
					
				Else
					Replace importe with _totdebe  - _tothaber 
					Replace debehaber with 'H'
					Replace haber with importe			
					Replace debe with 0
				endif	
				Replace ctaajuste with 1
				
				Select aux_ajusasiento
				*brow
				Sum debe to thisform.txttotal_debeasiento.Value
				Sum haber to thisform.txttotal_haberasiento.Value
				
				Set Order To CODAJUS   && ALLTRIM(STR(CTAAJUSTE))+CODIGO
				Go top
				
				Select aux_ajusdetalle
				Sum debe to thisform.txttotal_debedetalle.Value
				Sum haber to thisform.txttotal_haberdetalle.Value
				Set Order To FECHA   && DTOS(FECHA)+CODIGO
				Go top
				
				thisform.txtdiferencia.Value=Abs(thisform.txttotal_debedetalle.Value - thisform.txttotal_haberdetalle.Value)
				
			endif
			
			If _errorgral>0
				thisform.lab_errores.Visible=.t.
			endif
			
			
			*-- Habilito botón Grabar -----------------------------------------
			thisform.lblFECHA1.Visible=.t.
			thisform.txtfecha_asiento.Visible=.t.
			thisform.txtfecha_asiento.Value=thisform.txtfechacierre.Value
			thisform.labeldetalle.Visible= .T.
			thisform.b_grabar.Visible=.t.
			thisform.txtdetalle_asiento.Visible=.t.
			
			thisform.Refresh 
			
		Else  && Buscar IDCierre Generado.
			
			_idajuste=_idcierre
		
			sele ajustes_inflacion
			Set Order To idajuste
			Seek _idajuste
			If Found()
				thisform.txtperiodo.Value=percierre
				thisform.txtfechacierre.Value=feccierre
				thisform.txtcuenta_ajuste.Value=cuentaajus
				thisform.check_noaplicaajusteabienesdeusoenapertura.Value=nobsusoap
				thisform.check_noaplicaenasientosmarcados.Value=noajustar
				thisform.check_noaplicaajusteamortizacionenapertura.Value=noamortap
				thisform.check_noaplicaajusteenasientoCierre.Value=noapcierre
			EndIf
		
			Select ajustes_detalle
			Set Order To idajuste
			Seek _idajuste
			If Found()
				DO while not Eof() and idajuste=_idajuste
					Scatter memvar
					Select aux_ajusdetalle
					Append Blank
					Gather memvar
					
					Select ajustes_detalle
					skip
				enddo
			EndIf
				
		
			*-- Busco el detalle del Asiento --------------------------------------------------------------
			Select ajustes_asiento
			Set Order To idajuste
			Seek _idajuste
			If Found()
				DO while not Eof() and idajuste=_idajuste
					Scatter memvar
					Select aux_ajusasiento
					Append Blank
					Gather memvar
					
					Select ajustes_asiento
					skip
				enddo
			EndIf
		
			Select aux_ajusasiento
			*brow
			Sum debe to thisform.txttotal_debeasiento.Value
			Sum haber to thisform.txttotal_haberasiento.Value
			
			Set Order To CODAJUS   && ALLTRIM(STR(CTAAJUSTE))+CODIGO
			Go top
			
			Select aux_ajusdetalle
			Sum debe to thisform.txttotal_debedetalle.Value
			Sum haber to thisform.txttotal_haberdetalle.Value
			Set Order To FECHA   && DTOS(FECHA)+CODIGO
			Go top
			
			thisform.txtdiferencia.Value=Abs(thisform.txttotal_debedetalle.Value - thisform.txttotal_haberdetalle.Value)
				
			*-----------------------------------------------------------------------------------------------------------------------	
		
			thisform.b_grabar.Visible=.f.
			thisform.b_anular.Visible= .T.
			
		EndIf
		thisform.Refresh 
		
	ENDPROC

	PROCEDURE ver_error
		Select aux_ajusdetalle
		If error=1
			MessageBox(errordet,64,"Error")
		endif
	ENDPROC

	PROCEDURE b_anular.Click
		
		If Len(Alltrim(thisform.txtperiodover.Value))=0
			MessageBox("Ingrese el Periodo Contable que desea Consultar",64,"Control")
			return
		endif
		
		Select ajustes_inflacion
		Set Order To percierre
		Seek thisform.txtperiodover.Value
		If Found()
			thisform.txtidajuste.Value=idajuste
		Else
			MessageBox("No hay un Ajuste realizado para el Periodo",64,"Control")
			return
		endif
		thisform.ver_ajuste(thisform.txtidajuste.Value)
		
		
		If MessageBox("Confirma Anular el Ajuste por Inflación?",36,"Confirmación")=6
			thisform.anular_ajuste(thisform.txtidajuste.Value)
		EndIf
		
		
	ENDPROC

	PROCEDURE b_consultar.Click
		Select ajustes_inflacion
		Set Order To percierre
		Seek thisform.txtperiodover.Value
		If Found()
			thisform.txtidajuste.Value=idajuste
			thisform.txtperiodover.Value=percierre
			MessageBox("El Periodo Contable ya tiene realizado el Ajuste, ingrese en opción Ver Ajuste",64,"Control")
			*return
		EndIf
		
		thisform.ver_ajuste(0)
		
	ENDPROC

	PROCEDURE b_grabar.Click
		
		_error=0
		If Len(Alltrim(thisform.txtcuenta_filtro.Value))<>0
			MessageBox("Para realizar el Cierre no puede estar filtrado por Cuenta",64,"Control")
			thisform.txtcuenta_filtro.SetFocus
			return
		EndIf
		
		If thisform.txtfecha_asiento.Value={}
			MessageBox("Ingrese Fecha del Asiento Contable",64,"Control")
			thisform.txtfecha_asiento.SetFocus
			return
		EndIf
		If Len(Alltrim(thisform.txtdetalle_asiento.Value))=0
			MessageBox("Ingrese Detalle del Asiento",64,"Control")
			thisform.txtdetalle_asiento.SetFocus 
			return
		endif
		
		If MessageBox("Está seguro que desea Grabar el Asiento de Cierre",36,"Confirmación")=6
			*a==
			*-- Grabo en la tabla Cabecera de Ajustes -------------------------------------------------------------------------
			sele ajustes_inflacion
			Append Blank
			_idajuste=idajuste
			Replace fecha 		with Date()
			Replace feccierre 	with thisform.txtfechacierre.Value
			Replace percierre 	with thisform.txtperiodo.Value
			Replace perindaper	with thisform.txtperiodo_indaper.Value
			Replace nobsusoap 	with thisform.check_noaplicaajusteabienesdeusoenapertura.Value
			Replace noamortap	with thisform.check_noaplicaajusteamortizacionenapertura.Value
			Replace noapcierre	with thisform.check_noaplicaajusteenasientoCierre.Value
			Replace noajustar	with thisform.check_noaplicaenasientosmarcados.Value
			Replace cuentaajus 	with thisform.txtcuenta_ajuste.Value
			
			Replace opcrea 		with val(cod_op)
			Replace feccrea 	with Datetime()
			
			*-- Grabo en la tabla Detalle de Ajustes --------------------------------------------------------------------------
			Select aux_ajusdetalle
			Scan
				Scatter memvar
				Select ajustes_detalle
				Append Blank
				
				Gather memvar fields except iddetalled
				Replace idajuste with _idajuste
				
				Replace opcrea with val(cod_op)
				Replace feccrea with Datetime()
				
				
		
			EndScan
			
			*-- Grabo en la tabla Detalle de Asiento --------------------------------------------------------------------------
			Select aux_ajusasiento
			Scan
				Replace fecha with thisform.txtfecha_asiento.Value	&& Actualizo Fecha del Asiento.
				Scatter memvar
				Select ajustes_asiento
				Append Blank
				
				Gather memvar fields except iddetallea
				Replace idajuste with _idajuste
				
				Replace opcrea with val(cod_op)
				Replace feccrea with Datetime()
			
				Select aux_ajusasiento
				Replace idajuste with _idajuste
			EndScan
			
			*-- Grabo el Asiento -----------------------------------------------------------------------------------------------
			select detacont
			set filte to periodo=thisform.txtperiodo.Value && periodo_actual.periodo
			Set Order To PERASIENTO   && PERIODO+NROASIENTO
			go bott
			_asiento=ceros(str(val(nroasiento)+1,6),6)	
			_uni=sys(2015)
			
			append blank
			Replace ajusteinf 		with 1  && Marcar como Asiento de Ajuste
			Replace idajuste 		with _idajuste
			Replace fecha 			with thisform.txtfecha_asiento.Value
			Replace nroasiento 		with _asiento
			Replace periodo 		with thisform.txtperiodo.Value
			Replace detalle 		with thisform.txtdetalle_asiento.Value
			Replace unico 			with _uni
			Set Filter To
			
			Select aux_ajusasiento
			scan
				Scatter memvar
				
				Select movicont
				Append Blank
				Gather memvar
				
				Replace periodo 	with thisform.txtperiodo.Value
				Replace nroasiento 	with _asiento
				Replace unico 		with _uni
				Replace idajuste 	with _idajuste
				Replace ajusteinf	with 1			&& Marco el asiento como Asiento de Ajuste.
				
			EndScan
			*----------------------------------------------------------------------------------------------------------------------
			
			*----- Marco los asientos (encabezado) y Movimientos Ajustados del asiento con IdAjuste. ------------------------------
			Select aux_ajusdetalle
			Scan
				Scatter memvar
				_encontro_cab=.t.	
				
				Select detacont
				Set Order To unico
				Seek m.unicoori
				If Found()
				Else
					Set Order To perasiento
					Seek m.asientoori
					If Found()
						
					Else
						_encontro_cab=.f.	
					EndIf
				Endif
				If _encontro_cab
					Select detacont	
					Replace idajuste with _idajuste
				EndIf
				
				Select movicont
				*Set Order To unico
				*Seek m.unicoori
				Go m.registro
				*If Found()
				If Recno()=m.registro
					Replace idajuste with _idajuste
					If m.registro=185297 && unico='_53L11EBFP'
						*a==
					EndIf
					Select aux_ajusdetalle
					Replace idajuste with _idajuste
				Endif
			
			EndScan
			*-----------------------------------------------------------------------------------------------------------------------	
			MessageBox("Cierre Realizado Correctamente",64,"Aviso")
			
			thisform.txtidajuste.Value=_idajuste
			thisform.txtperiodover.Value=thisform.txtperiodo.Value
			*-----------------------------------------------------------------------
			
			thisform.b_grabar.Visible=.f.
			
			
			
		endif
		
	ENDPROC

	PROCEDURE grdAux_ajusdetalle.Column1.Text1.DblClick
