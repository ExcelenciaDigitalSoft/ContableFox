	PROCEDURE anular_ajuste
		Parameters _idcierre
		
		_idajuste=_idcierre
		*a==
		
		sele ajustes_inflacion
		Set Order To idajuste
		Seek _idajuste
		If Found()
			
			Replace opanula with val(cod_op)
			Replace fecanula with Datetime()
			Delete
		EndIf
		
		*-- Anulo Ajuste en la tabla Detalle de Movimientos de Ajustes --------------------------------------------------------------------------
		
		Select ajustes_detalle
		Set Order To idajuste
		Seek _idajuste
		If Found()
			DO while not Eof() and idajuste=_idajuste
				Replace opanula with val(cod_op)
				Replace fecanula with Datetime()
				Delete
				skip
			enddo
		EndIf
			
		
		*-- Anulo Ajuste en la tabla Detalle de ASientos --------------------------------------------------------------------------
		
		Select ajustes_asiento
		Set Order To idajuste
		Seek _idajuste
		If Found()
			DO while not Eof() and idajuste=_idajuste
				Replace opanula with val(cod_op)
				Replace fecanula with Datetime()
				Delete
				skip
			enddo
		EndIf
		
		*-- Anulo el Asiento -----------------------------------------------------------------------------------------------
		select detacont
		Set Filter To
		Set Order To idajuste
		Seek _idajuste
		If Found()
			DO while not Eof()  and idajuste=_idajuste
				*if	idajuste=_idajuste
					If ajusteinf=1	&& si es el Asiento de Ajuste lo anulo.
						Replace opanula with val(cod_op)
						Replace fecanula with Datetime()
						Delete
					Else  && Si son los asientos marcados, los desmarco para el nuevo Ajuste.
						*Replace idajuste with 0
					EndIf
				*endif
				Skip
			enddo
		EndIf
		Select detacont
		Set Filter To idajuste=_idajuste
		Scan
			Replace idajuste with 0
		endscan
		Set Filter To
		
		
		select movicont
		Set Filter To 
		Set Order To idajuste
		Seek _idajuste
		If Found()
			DO while not Eof() and idajuste=_idajuste
				*if	idajuste=_idajuste
					If ajusteinf=1	&& si es el Asiento de Ajuste lo anulo.
						Replace opanula with val(cod_op)
						Replace fecanula with Datetime()
						Delete
					Else  && Si son los asientos marcados, los desmarco para el nuevo Ajuste.
						*Replace idajuste with 0
					EndIf
				*EndIf
				
				Skip
			enddo
		EndIf
		Select movicont
		Set Filter To idajuste=_idajuste
		Scan
			Replace idajuste with 0
		endscan
		Set Filter To 
		
		*-----------------------------------------------------------------------------------------------------------------------	
		MessageBox("Ajuste Anulado Correctamente",64,"Aviso")
		
		thisform.txtidajuste.Value=0
		
		Select aux_ajusdetalle
		TableRevert(.t.)
		
		Select aux_ajusasiento
		TableRevert(.t.)
		
		Store 0 to thisform.txttotal_debeasiento.Value,thisform.txttotal_debedetalle.Value,thisform.txttotal_haberasiento.Value,thisform.txttotal_haberdetalle.Value,thisform.txtdiferencia.value
		
		thisform.Refresh 
		*-----------------------------------------------------------------------
		
		thisform.b_grabar.Visible=.f.
		thisform.b_anular.Visible= .T.
		thisform.Refresh 
	ENDPROC

	PROCEDURE imprimir
		Parameters _tipoimprimir
		
		
		Select aux_ajusdetalle
		Set Order To FECHA   && DTOS(FECHA)+CODIGO
		
		If Reccount()>0
			If _tipoimprimir=1
				
				Report Form detalle_ajuste NOCONSOLE PREVIEW 
			Else
				cArch=''
				cArch=Putfile("Nombre a Guardar:","DetalleMovimientosAjustados.Xls","XLS")
				Select aux_ajusdetalle
				If Len(Alltrim(cArch))#0
					Copy field fecha,nroasiento,asi_inicio,leyenda,codigo,nomcuenta,perinicio,indinicio,percierre,indcierre,coeficient,impbase,impajuste,debehaber,debe,haber;
					To (cArch) Type Xls
				Endif	
			
			EndIf
		Else
			MessageBox("No hay Datos para Mostrar",48,"Control")
		EndIf
			
	ENDPROC

	PROCEDURE imprimir_asiento
