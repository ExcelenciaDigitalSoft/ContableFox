	PROCEDURE Xpbutton6.Click
		
		_error=0
		
		If MessageBox("Está seguro que desea Grabar el Asiento de Cierre",36,"Confirmación")=6
			a==
			
			
			*----- Marco los asientos (encabezado) y Movimientos Ajustados del asiento con IdAjuste. ------------------------------
			Select aux_ajusdetalle
			brow
			Scan
				Scatter memvar
				
				_encontro_cab=.t.	
				_idajuste=m.idajuste
				
				Select detacont
				Set Order To unico
				Seek m.unicoori
				If Found()
				Else
					Set Order To perasiento
					Seek m.asientoori
					If Found()
						
					Else
						_encontro_cab=.f.	
					EndIf
				Endif
				If _encontro_cab
					Select detacont	
					If idajuste=0
						Replace idajuste with _idajuste
					endif
				EndIf
				
				Select movicont
				*Set Order To unico
				*Seek m.unicoori
				Go m.registro
				*If Found()
				If Recno()=m.registro
					If idajuste=0
						Replace idajuste with _idajuste
					EndIf
					
					If m.registro=185297 && unico='_53L11EBFP'
						*a==
					EndIf
		
				Endif
			
			EndScan
			*-----------------------------------------------------------------------------------------------------------------------	
			MessageBox("Cierre Realizado Correctamente",64,"Aviso")
			
		
			
			
			
		endif
		
	ENDPROC

