		thisform.refresh
	ENDPROC

	PROCEDURE Todos.Click
		If thisform.m=0
			thisform.m=1
		else	
			thisform.m=0
		EndIf
		Select aux_detfac
		If thisform.m=1
			Replace all marcado with 1
			this.Caption="Desmarcar Todos"		
		Else
			Replace all marcado with 0
			this.Caption="Marcar Todos"	
		EndIf
		thisform.grdPendientes.SetAll("dynamicbackcolor","IIF(aux_detfac.marcado=1,RGB(174,215,255),RGB(255,255,255))","Column")
		thisform.grdPendientes.Refresh
