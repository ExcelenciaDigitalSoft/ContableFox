	PROCEDURE Confirmar.Click
		If MessageBox("Confirma Liquidar Obras Sociales Seleccionadas",36,"Confirmar")=6
			*Select aux_detfacliq
			*=TableRevert(.t.)
			Select aux_detfac
			Go top
			Scan
				reg=registro
				If marcado=1
					scatter memvar
					m.registro=reg
					Select aux_detfacliq
					Append Blank
					Gather memvar
				EndIf
				Select aux_detfac		
			EndScan
			c=0
			Select aux_detfacliq
			Go top
			Scan
				c=c+1
			endscan	
			If c=0
				Select aux_detfacliq
				Append Blank
			EndIf
			thisform.txtNompro.SetFocus
		endif
		Select aux_detfac
		=TableRevert(.t.)
		thisform.grdAux_det.refresh
		thisform.shape3.Visible=.f.
		thisform.label17.Visible=.f.
		thisform.label18.Visible=.f.
		thisform.label19.Visible=.f.
		thisform.grdPendientes.Visible=.f.
		thisform.confirmar.Visible=.f.	
		thisform.salir.Visible=.f.	
		thisform.todos.Visible=.f.	
		thisform.texttot.visible=.f.
		thisform.refresh
		
	ENDPROC

	PROCEDURE grdPendientes.Column8.marcar.Click
		Select aux_detfac
		If aux_detfac.marcado=0
			replace aux_detfac.marcado with 1
		Else
			replace aux_detfac.marcado with 0
		endif
		thisform.grdPendientes.SetAll("dynamicbackcolor","IIF(aux_detfac.marcado=1,RGB(174,215,255),RGB(255,255,255))","Column")
		thisform.grdPendientes.Refresh
	ENDPROC

	PROCEDURE Salir.Click
