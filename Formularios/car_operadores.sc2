			keyb '{SHIFT+RIGHTARROW}'
		endfor
	ENDPROC

	PROCEDURE txtCodigo.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
			do form busqueda with 'operadores','','codigo','nombre' to this.value
		endif
		
	ENDPROC

	PROCEDURE txtCodigo.LostFocus
		if val(this.value)#0
			this.value=ceros(this.value,3)
			select operadores
			set order to codigo
			seek this.value
			if !found()
				thisform.txtnombre.value=''
				thisform.txtdireccion.value=''
				thisform.txttelefono.value=''
				thisform.text1.enabled=.f.
				thisform.text2.enabled=.f.
				thisform.text3.enabled=.f.
				thisform.text6.value=''
			else
				thisform.txtnombre.value=nombre
				thisform.txtdireccion.value=direccion
				thisform.txttelefono.value=telefono	
				thisform.text6.value=perfil
				thisform.text4.value=coder10(nivel,2,.f.)
				thisform.Check1.value = revisa
				thisform.Check2.value = autoriza
				thisform.text1.enabled=.t.
				thisform.text2.enabled=.t.
				thisform.text3.enabled=.t.
			endif
		else
			if lastkey()=13 or lastkey()=9 or lastkey()=24
				=messagebox('Debe Cargar un Codigo',48,'Control')
				node
			endif	
		endif
	ENDPROC

