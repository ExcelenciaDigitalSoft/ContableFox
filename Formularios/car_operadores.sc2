	PROCEDURE Text5.LostFocus
		if !empty(this.value)
			select contraseña
			go top
			if alltr(coder10(password,.f.))==alltr(this.value)
				select operadores
				set order to codigo
				seek thisform.txtcodigo.value
				if found()
					repl nivel with coder10(thisform.text4.value,2,.t.)
					messagebox("El nivel de Acceso fue Actualizado.",48,"Seguridad")
				endif
			else	
				messagebox("Llave de Seguridad Incorrecta."+chr(13)+"No se ha efectuado el cambio de Nivel",48,"Seguridad")	
			endif
		endif	
		
	ENDPROC

	PROCEDURE Text6.LostFocus
		if val(this.value)>0
			this.value=ceros(this.value,3)
			
		endif
	ENDPROC

	PROCEDURE txtCodigo.GotFocus
