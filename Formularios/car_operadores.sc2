	PROCEDURE Init
		select operadores
		set order to codigo
		go bott
		thisform.label1.caption='Ultimo Cargado: '+ceros(codigo,6)
	ENDPROC

	PROCEDURE Command1.Click
		release thisform
		
	ENDPROC

	PROCEDURE Command2.Click
		if messagebox('Confirma Grabar Datos',36,'Grabar')=6
			if !empty(thisform.txtnombre.value)
				select operadores
				set order to codigo
				seek thisform.txtcodigo.value
				if !found()
					append blank
					repl contraseña with coder10('123',10,.t.)
					messagebox('Su Contraseña es: 123.Se le recomienda Cambiarla.',48,'Seguridad')
				endif
				repl codigo with thisform.txtcodigo.value
				repl nombre with thisform.txtnombre.value
				repl direccion with thisform.txtdireccion.value
				repl telefono with thisform.txttelefono.value
				repl nivel with coder10(thisform.text4.value,2,.t.)
				repl perfil with thisform.text6.value
				replace revisa with thisform.check1.value
				replace autoriza with thisform.check2.value
				thisform.txtnombre.value=''
				thisform.txtdireccion.value=''
				thisform.txttelefono.value=''
				thisform.txtcodigo.value=space(3)
				select operadores
				set order to codigo
				go bott
				thisform.label1.caption='Ultimo Cargado: '+ceros(codigo,6)
				thisform.txtcodigo.setfocus
			else
				=messagebox('Debe Cargar el Nombre del Operador.',48,'Control')	
				thisform.txtnombre.setfocus
			endif	
		endif
	ENDPROC

	PROCEDURE Command3.Click
		if messagebox('Confirma Eliminar Datos',36,'Eliminar')=6
			select operadores
			set order to codigo
			seek thisform.txtcodigo.value
			if found()
				dele
			endif
			thisform.txtnombre.value=''
			thisform.txtdireccion.value=''
			thisform.txttelefono.value=''
			thisform.txtcodigo.value=space(3)
			select operadores
			set order to codigo
			go bott
			thisform.label1.caption='Ultimo Cargado: '+ceros(codigo,6)
			thisform.txtcodigo.setfocus
		endif	
	ENDPROC

	PROCEDURE Command4.Click
		if messagebox('Confirma Cambiar Contraseña',36,'Seguridad')=6
			if thisform.text1.value#space(6)
				con=''
				select operadores
				set order to codigo
				seek thisform.txtcodigo.value
				con=coder10(operadores.contraseña,.f.)
				if alltr(con)==alltr(thisform.text1.value)
					if alltr(thisform.text2.value)==alltr(thisform.text3.value)
						repl contraseña with alltr(coder10(alltr(thisform.text2.value),10,.t.))
						=messagebox('Contraseña actualizada',48,'CONTROL')
					else
						=messagebox('La Nueva contraseña No coinside con su verificacion. Confirme nuevamente',48,'Control')	
						thisform.text2.value=''
						thisform.text3.value=''
						thisform.text2.setfocus
					endif
				else	
					=messagebox('Contraseña no Valida.',48,'Seguridad')
					thisform.text2.value=''
					thisform.text3.value=''
					thisform.text1.value=''
				endif
			else
				=messagebox('Ingrese contraseña ',48,'Seguridad')
				thisform.text1.value=space(6)
				thisform.text2.value=space(6)
				thisform.text3.value=space(6)
				thisform.text1.setfocus
			endif	
		endif	
	ENDPROC

