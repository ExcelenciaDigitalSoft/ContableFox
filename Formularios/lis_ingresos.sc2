	PROCEDURE Text1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		if betw(nkeycode,65,90) or betw(nkeycode,97,122)
			do form busqueda with 'Bancos','','codigo','nombre' to this.value
		endif
	ENDPROC

	PROCEDURE Text1.LostFocus
		if val(this.value)#0
			this.value=ceros(this.value,3)
			select bancos
			set orde to codigo
			seek this.value
			if found()
				thisform.text4.value=bancos.nombre
			else	
				thisform.text4.value=""
				messagebox("Este Cuenta de Tesoreria no Existe",48,"Control")
			endif
		else
			If lastkey()=13 or lastkey()=24 or lastkey()=9
				thisform.text4.value="Todas las Cuentas"
			EndIf	
		Endif	
	ENDPROC

	PROCEDURE Text4.KeyPress
