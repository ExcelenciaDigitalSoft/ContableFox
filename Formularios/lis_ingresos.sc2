				where betw(fecha,thisform.text2.value,thisform.text3.value) and cuenta_teso=thisform.text1.value ;
				order by cuenta_teso,fecha ;
				into cursor Consulta
			else   && Todos los tipos de Ingresos
				select Numero,cuenta_teso,Nom_cuenta,Fecha,Detalle,Tarjeta,Cheque,Efectivo,Importe ;
				from ingresos ;
				where betw(fecha,thisform.text2.value,thisform.text3.value) ;
				order by cuenta_teso,fecha ;
				into cursor Consulta			
			ENDIF
