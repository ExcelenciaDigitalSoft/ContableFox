	PROCEDURE Grid1.Column6.Command3.Click
		thisform.grabar_oc("aux_voc2Antes","aux_vocAntes",3)
		
		return
		
		
		
		sele aux_voc2Antes
		if val(codpro)<>0
			If grabado=1
				MessageBox("Los datos ya se encuentra Grabados",48,"Control")
				return
			EndIf
			
			If messagebox("Desea Grabar Datos para "+allt(aux_voc2Antes.nombre),36,"Confirmacion")<>6
				return
			endif
			
			if aux_voc2Antes.grabado=0
				thisform.recuperar_datos(codpro,tipo,'aux_vocAntes')
		
				sele aux_vocGrabar
				if recc()>0
					*--- Recuperar el ultimo nro grabado y sumarle 1  ---------
					_ultnro=''
					if aux_voc2Antes.tipo=1
						_ultnro= 'OC'+aux_voc2Antes.codpro+right(Alltrim(thisform.txtAño.Value),2)+Alltrim(thisform.txtMes.Value)+'04'
					else
						if aux_voc2Antes.tipo=2
							_ultnro= 'OC'+aux_voc2Antes.codpro+right(Alltrim(thisform.txtAño.Value),2)+Alltrim(thisform.txtMes.Value)+'05'
						else
							_ultnro= 'OC'+aux_voc2Antes.codpro+right(Alltrim(thisform.txtAño.Value),2)+Alltrim(thisform.txtMes.Value)+'06'
						endif
					endif
					
					*----------------------------------------------------------
					*-- Grabar en dbf en FacCom
					fec = Gomonth(Ctod('01/'+Alltrim(thisform.txtMes.Value)+'/'+Alltrim(thisform.txtAño.Value)),1)-1
					Select faccom  
					Append Blank
					Replace cliente with '001'
					
					&& En factura compras grabo un registro con el total del periodo.
					* Este numero va a funcionar como Clave para el Detalle.
					Replace numero with  _ultnro  && 'OC'+aux_voc2Antes.codpro+Alltrim(thisform.txtAño.Value)+Alltrim(thisform.txtMes.Value)
					_Clave=numero 
					Replace fecha with fec
					Replace fecha_contable with fec
					Replace periodo with Alltrim(thisform.txtAño.Value)+Alltrim(thisform.txtMes.Value)
					Replace proveedor with aux_voc2Antes.codpro
					Replace nombre with aux_voc2Antes.nombre
					Replace exento with aux_voc2Antes.total
					Replace neto with aux_voc2Antes.total
					Replace total with aux_voc2Antes.total  && Total es el valor que puede modificar Marcela Costa, y pagar lo que ella modifique.
					Replace ctacte with aux_voc2Antes.total
					*replace total_oc with aux_voc2Antes.total_oc && Valor segun el descargo de Dinero.
					m.total_oc=aux_voc2Antes.total_oc
					Replace per_oc with Alltrim(thisform.txtAño.Value)+Alltrim(thisform.txtMes.Value)
					repl total_imp with aux_voc2Antes.total_imp
					repl tipo_oc with aux_voc2Antes.tipo
					
					scatter memvar
								
					*-- Grabar en sql en   OrdenPagoProveedores y recuperar Id.
					*if .f.
					_cp=m.proveedor
					_nc=m.numero
					_fe=m.fecha
					_tt=m.total
					_to=m.total_oc
					_oc=0
					_fc=datetime()
					_ti=m.total_imp
					_toc=m.tipo_oc
					
					_cadena="INSERT INTO OrdenPagoProveedores(CodProveedor,NumeroComprobante,Fecha,Total,Total_OC,OpCrea,FecCrea,Total_Imp,Tipo_oc)values(?_cp,?_nc,?_fe,?_tt,?_to,?_oc,?_fc,?_ti,?_toc);Select  SCOPE_IDENTITY() as ultimo "
					_error=ThisForm.Msde.sql_identity("IdGrabado",_cadena)	&& Graba y Recupera el Id recientemente insertado en el cursor que se llama IdGrabado.
					
					IF _error=0  && No hubo Error al grabar en sql
						
						SELECT IdGrabado  && Genera un cursor con el Id del Sql recien insertado.
						GO top
		
						IF IdGrabado.ultimo<>0
							*-- Grabar en sql en DetalleOrdenPagoProveedores.
							_idordenpago=IdGrabado.ultimo
							_oc=0
							_fc=datetime()
							
							sele aux_vocGrabar
							scan	
								if codpro=m.proveedor and tipo=aux_voc2Antes.tipo   && me fijo q tipo estoy grabando: Periodo 1 o Recupero 2
									_idpago=idpago
									_cadena="INSERT INTO DetalleOrdenPagoProveedores(IdOrdenPago,IdDetallePago,OpCrea,FecCrea)values(?_idordenpago,?_idpago,?_oc,?_fc);Select  SCOPE_IDENTITY() as ultimo "
									_error=ThisForm.Msde.sql_identity("IdGrabado",_cadena)	&& Graba y Recupera el Id recientemente insertado en el cursor que se llama IdGrabado.
									if _error<>0
									
									endif
								endif
							endscan
						endif
					else
						sele faccom
						dele   && elimino el registro porque dio error al grabar en sql.
					endif
					*endif
					messagebox("Los Datos fueron Grabaron",64,"Control")		
					sele aux_voc2Antes
					repl grabado with 1
					
				else
					messagebox("No hay Datos para Grabar",48,"Control")
				endif
			else
				sele pagosprov
				set order to pronum
				seek aux_voc2Antes.codpro+aux_voc2Antes.numero
				if found()
					messagebox("El comprobante ya tiene pagos imputados, no se puede modificar",48,"Control")
					return
				endif
				
				sele faccom
				set order to pronum
				seek aux_voc2Antes.codpro+aux_voc2Antes.numero
				if found()
					
					Replace exento with aux_voc2Antes.total
					Replace neto with aux_voc2Antes.total
					Replace total with aux_voc2Antes.total  && Total es el valor que puede modificar Marcela Costa, y pagar lo que ella modifique.
					Replace ctacte with aux_voc2Antes.total
					repl total_imp with aux_voc2Antes.total_imp
					repl tipo_oc with aux_voc2Antes.tipo
				endif
				
			endif
			
		else
			messagebox("Codigo de Proveedor Incorrecto",48,"Control")
		endif
		
		thisform.refresh
	ENDPROC

