								thisform.msde.vervista("V_Comecompras",_consulta_peractual)
								thisform.cargar_periodo("v_comecompras","aux_voc","aux_voc2",1,0,'','PERIODO')
							EndIf				
						EndIf
						
						if _busca_periodoant
							If .t. && Len(Alltrim(_grabado_perant_pagomenorper))=0  && Si no está grabado consulto lo pendiente.
								thisform.msde.vervista("V_Comecompras_perant",_consulta_perant)
