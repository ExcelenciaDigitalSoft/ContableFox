					EndIf
					
					if _busca_periodo
						_consulta_peractual="select 0 as otroper,* from v_pagosgeneralesrendicion where tipo=2 and idcomercio in ("+allt(thisform.codigos_asociados)+") and (aniopago+mespago='"+_periodo+"' and &_condpagoIgualPeriodo) and IdOrdenPago=0 order by idpago,iddetalledeuda,aniocuota,mescuota,aniopago,mespago"
					endif
					
					if _busca_periodoant
						_consulta_perant="select 0 as otroper,* from v_pagosgeneralesrendicion where tipo=2 and idcomercio in ("+allt(thisform.codigos_asociados)+") and (aniopago+mespago<'"+thisform.txtaño.value+thisform.txtmes.value+"' and &_condpagoMenorPeriodo) and IdOrdenPago=0 order by idpago,iddetalledeuda,aniocuota,mescuota,aniopago,mespago"
						*-- Cuotas de Periodos Anteriores - Pago en periodo Actual.
						endif
					
					if	_busca_adelantos=.t.
