	PROCEDURE grabar_oc
		Parameters _nomtabla,_nomtabladetalle,_suma
		*a==
		sele &_nomtabla
		_np=_nomtabla+'.codpro'
		_nu=_nomtabla+'.numero'
		_codpro=&_np &&codpro
		_numero=&_nu
		if val(_codpro)<>0
			If grabado=1
				MessageBox("Los datos ya se encuentra Grabados",48,"Control")
				return
			EndIf
			
			If messagebox("Desea Grabar Datos para "+allt(nombre),36,"Confirmacion")<>6
				return
			endif
			
			if grabado=0
				_tipo=tipo
				thisform.recuperar_datos(_codpro,tipo,_nomtabladetalle)
				
				sele aux_vocGrabar
				if recc()>0
					Select &_nomtabla
					*--- Recuperar el ultimo nro grabado y sumarle 1  ---------
					_ultnro=ceros(Str(_tipo+_suma ),2) && segun el tipo es la terminacion  1,2,3
					Select numero from faccom where tipo_oc=_tipo+_suma and Left(numero,12)='OC'+_codpro+right(Alltrim(thisform.txtAño.Value),2)+Alltrim(thisform.txtMes.Value) order by numero desc  into cursor Cargados
					*brow
					Select Cargados
					If Reccount()>0 
						if Reccount()<10
							If Reccount()=1
								Go top
								_ultnro=ceros(Str(_tipo+_suma)+'0',2)  && para 
							Else
								_ultnro=ceros(Str(Val(Right(numero,2))+1),2)
							endif
						Else
							MessageBox("No se pueden grabar tantas OC del mismo Tipo para el Periodo",48,"Control")
							return
						endif
					endif
					_ultnro='OC'+_codpro+right(Alltrim(thisform.txtAño.Value),2)+Alltrim(thisform.txtMes.Value)+_ultnro
					*if aux_voc2.tipo=1
					*	_ultnro= 'OC'+aux_voc2.codpro+right(Alltrim(thisform.txtAño.Value),2)+Alltrim(thisform.txtMes.Value)+'01'
					*else
					*	if aux_voc2.tipo=2
					*		_ultnro= 'OC'+aux_voc2.codpro+right(Alltrim(thisform.txtAño.Value),2)+Alltrim(thisform.txtMes.Value)+'02'
					*	else
					*		_ultnro= 'OC'+aux_voc2.codpro+right(Alltrim(thisform.txtAño.Value),2)+Alltrim(thisform.txtMes.Value)+'03'
					*	endif
					*endif
					
					*----------------------------------------------------------
					*-- Grabar en dbf en FacCom
					fec = Gomonth(Ctod('01/'+Alltrim(thisform.txtMes.Value)+'/'+Alltrim(thisform.txtAño.Value)),1)-1
					Select faccom  
					Append Blank
					Replace cliente with '001'
					Select &_nomtabla
					&& En factura compras grabo un registro con el total del periodo.
					* Este numero va a funcionar como Clave para el Detalle.
					Replace faccom.numero with  _ultnro  && 'OC'+aux_voc2.codpro+Alltrim(thisform.txtAño.Value)+Alltrim(thisform.txtMes.Value)
					_Clave=numero 
					Replace faccom.fecha with fec
					Replace faccom.fecha_contable with fec
					Replace faccom.periodo with Alltrim(thisform.txtAño.Value)+Alltrim(thisform.txtMes.Value)
					Replace faccom.proveedor with _codpro
					Replace faccom.nombre with nombre
					Replace faccom.exento with total
					Replace faccom.neto with total
					Replace faccom.total with total  && Total es el valor que puede modificar Marcela Costa, y pagar lo que ella modifique.
					Replace faccom.ctacte with total
					m.total_oc=total_oc
					Replace faccom.per_oc with Alltrim(thisform.txtAño.Value)+Alltrim(thisform.txtMes.Value)
					repl faccom.total_imp with total_imp
					repl faccom.tipo_oc with tipo+_suma
					
					Select faccom
					scatter memvar
								
					*-- Grabar en sql en   OrdenPagoProveedores y recuperar Id.
					*if .f.
					_cp=m.proveedor
					_nc=m.numero
					_fe=m.fecha
					_tt=m.total
					_to=m.total_oc
					_oc=0
					_fc=datetime()
					_ti=m.total_imp
					_toc=m.tipo_oc
					
					_cadena="INSERT INTO OrdenPagoProveedores(CodProveedor,NumeroComprobante,Fecha,Total,Total_OC,OpCrea,FecCrea,Total_Imp,Tipo_oc)values(?_cp,?_nc,?_fe,?_tt,?_to,?_oc,?_fc,?_ti,?_toc);Select  SCOPE_IDENTITY() as ultimo "
					_error=ThisForm.Msde.sql_identity("IdGrabado",_cadena)	&& Graba y Recupera el Id recientemente insertado en el cursor que se llama IdGrabado.
					
					IF _error=0  && No hubo Error al grabar en sql
						
						SELECT IdGrabado  && Genera un cursor con el Id del Sql recien insertado.
						GO top
		
						IF IdGrabado.ultimo<>0
							*-- Grabar en sql en DetalleOrdenPagoProveedores.
							_idordenpago=IdGrabado.ultimo
							_oc=0
							_fc=datetime()
							
							sele aux_vocGrabar
							scan	
								if codpro=m.proveedor and tipo=_tipo   && me fijo q tipo estoy grabando: Periodo 1 o Recupero 2
									_idpago=idpago
									_cadena="INSERT INTO DetalleOrdenPagoProveedores(IdOrdenPago,IdDetallePago,OpCrea,FecCrea)values(?_idordenpago,?_idpago,?_oc,?_fc);Select  SCOPE_IDENTITY() as ultimo "
									_error=ThisForm.Msde.sql_identity("IdGrabado",_cadena)	&& Graba y Recupera el Id recientemente insertado en el cursor que se llama IdGrabado.
									if _error<>0
									
									endif
								endif
							endscan
						endif
					else
						sele faccom
						dele   && elimino el registro porque dio error al grabar en sql.
					endif
					*endif
					messagebox("Los Datos fueron Grabaron",64,"Control")		
					sele &_nomtabla
					repl grabado with 1
					
				else
					messagebox("No hay Datos para Grabar",48,"Control")
				endif
			Else
				Select &_nomtabla
		
				sele pagosprov
				set order to pronum
				seek _codpro+_numero
				if found()
					messagebox("El comprobante ya tiene pagos imputados, no se puede modificar",48,"Control")
					return
				endif
				
				sele faccom
				set order to pronum
				seek _codpro+_numero
				if found()
					Select &_nomtabla
					Replace faccom.exento 	with total
					Replace faccom.neto 	with total
					Replace faccom.total 	with total  && Total es el valor que puede modificar Marcela Costa, y pagar lo que ella modifique.
					Replace faccom.ctacte 	with total
					replace faccom.total_imp with total_imp
					replace faccom.tipo_oc 	with tipo
				endif
				
			endif
			
		else
			messagebox("Codigo de Proveedor Incorrecto",48,"Control")
		endif
		
		thisform.refresh
	ENDPROC

