			endif
		ENDIF
		*------------------------------------------------------------------------
		
		DO case
			Case Alltrim(_tipo_pagos)=='PERIODO'
				*thisform.txttotgeneral.value=thisform.text1.value  + thisform.txtordant.value + thisform.txtnorendidas.value
				Select &_nomtabladestino
