	PROCEDURE Xpbutton2.Click
		if messagebox('Desa Exportar a Excel ',36,'Asistente')=6
			if val(thisform.text1.value)#0
				if thisform.o.o1.value=1
					select * ,verfield("Proveedores",proveedor_ingreso,"Cuit","Codigo") as cuit,verfield("Proveedores",proveedor_ingreso,"brutos","Codigo") as brutos ;
					from retenciones and where betw(fec_ing,thisform.text2.value,thisform.text3.value) and fec_egr={};
					and codigo=thisform.text1.value ;
					order by codigo,fec_ing ;
