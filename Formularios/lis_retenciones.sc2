					select consulta
				endif		
			else   && Todos los tipos de Retenciones
				if thisform.o.o1.value=1
					select * ,verfield("Proveedores",proveedor_ingreso,"Cuit","Codigo") as cuit,verfield("Proveedores",proveedor_ingreso,"brutos","Codigo") as brutos ;
					from retenciones and where betw(fec_ing,thisform.text2.value,thisform.text3.value) and fec_egr={};
					order by codigo,fec_ing ;
					into cursor consulta
					select consulta
				endif	
				if thisform.o.o2.value=1
					select * ,verfield("Proveedores",proveedor_ingreso,"Cuit","Codigo") as cuit,verfield("Proveedores",proveedor_ingreso,"brutos","Codigo") as brutos ;
					from retenciones and where betw(fec_egr,thisform.text2.value,thisform.text3.value) and fec_egr#{};
					order by codigo,fec_egr ;
					into cursor consulta
					select consulta
				endif	
				if thisform.o.o3.value=1
					select * ,verfield("Proveedores",proveedor_ingreso,"Cuit","Codigo") as cuit,verfield("Proveedores",proveedor_ingreso,"brutos","Codigo") as brutos ;
					from retenciones and where betw(fec_ing,thisform.text2.value,thisform.text3.value) ;
					order by codigo,fec_ing ;
					into cursor consulta
					select consulta
				endif				
			Endif	
			select excel_reten
			=tablerevert(.t.)
			select consulta
			go top
			scan
				scatter memvar
				select excel_reten
				append blank
				gather memvar
				select consulta
			endscan
			excel("excel_reten")
		endif	
	ENDPROC

