		Set Order To CLAVE   
		Seek ThisForm.SelCar.SelectedItem.Key
		If !Found()
			Go Bottom
			Skip
		EndIf
		ThisForm.Refresh
	ENDPROC

	PROCEDURE selcar.Init
		this.ImageList = thisform.lisima.oBJECT
		=sys(2333, 0)
		
	ENDPROC

	PROCEDURE selcar.KeyPress
		*** Evento de control OLE ***
		LPARAMETERS keyascii
		if keyascii = 27
			keyboard "{Esc}" && solución para que no se cuelgue
		endif
		
	ENDPROC

	PROCEDURE selcar.NodeClick
		*** ActiveX Control Event ***
		LPARAMETERS node
		Store .F. To ThisForm.Cmd1.Enabled,ThisForm.Cmd2.Enabled
		Select Menu_Osp
		Set Order To Clave
		Seek ThisForm.SelCar.SelectedItem.Key
		If Found()
			If Len(Alltrim(Padre))=0
				_Act=Substr(Clave,2,2)
				Set Order To Menu
				Go Top
				_pri=Substr(Clave,2,2)
				Go Bottom
				_ult=Substr(Clave,2,2)
				Set Order To Clave
				If _Act<>_Pri
					ThisForm.Cmd1.Enabled=.T.
				Endif
				If _Act<>_Ult
					ThisForm.Cmd2.Enabled=.T.
				Endif
			Else
				_Clave=Clave
				_Padre=Padre
				_act=Right(Clave,2)
				Set Filter To Padre=_Padre
				Go Top
				_pri=Right(Clave,2)
				Go Bottom
				_ult=Right(Clave,2)
				Set Filter To 
				If _Act<>_Ult
					ThisForm.Cmd2.Enabled=.T.
				Endif
				If _Act<>_pri
					ThisForm.Cmd1.Enabled=.T.
				Endif
			Endif
		Endif
		Select Menu_Osp
		Set Order To CLAVE   
		Seek ThisForm.SelCar.SelectedItem.Key
		If !Found()
			Go Bottom
			Skip
		EndIf
		ThisForm.Refresh
		ThisForm.Refresh
	ENDPROC

	PROCEDURE selcar.OLEDragDrop
		*** ActiveX Control Event ***
		LPARAMETERS data, effect, button, shift, x, y
		Local lcData, loNode
		If oData.GetFormat(1) && text-format
		   This.DropHighlight = .NULL.
		   effect =2
		   lcData = oData.GetData(1) && text-format
		   loNode = This.HitTest( 15*x,15*y )
		   MessageBox( 'Node at ('+TRANSFORM(x)+','+transform(y)+') Got data: ' + lcData )
		   If VarType(loNode)='O'
		      This.Nodes.Add( loNode, 4, "A" + SYS(3), lcData )
		      loNode.Expanded = .T.
		   Else
		      This.Nodes.Add( .null., 0, "A" + SYS(3), lcData )
		   Endif
		Endif
	ENDPROC

	PROCEDURE selcar.OLEDragOver
		*** ActiveX Control Event ***
		LPARAMETERS data, effect, button, shift, x, y, state
		Local loNode
		If oData.GetFormat(1) && 1=text
		   loNode = THIS.HitTest( 15*x, 15*y )
		   effect = 2
		   THIS.DropHighlight = loNode
		Else
		   effect = 0
		Endif
	ENDPROC

ENDDEFINE
