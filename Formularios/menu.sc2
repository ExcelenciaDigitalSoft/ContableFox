	PROCEDURE carga
		ThisForm.SelCar.Nodes.Clear()
		Select Menu_Osp
		Set Order To Menu
		Go Top
		Do While Not Eof()
			m.Registro = Recno("Menu_Osp")
			ThisForm.SelCar.Nodes.Add( , ,  Clave, Prompt , 1, 1)
			ThisForm.SelCar.Nodes(Clave).Expanded = .F.
			ThisForm.SelCar.Nodes(Clave).Tag = 1
			Set Order To Clave
			ThisForm.Expande(Clave)
			Set Order To Menu
			Go m.Registro In Menu_Osp
			Skip 1 In Menu_Osp
		Enddo
		ThisForm.Refresh()
		
	ENDPROC

	PROCEDURE expande
		Lparameters m.Padre
		Local m.Registro, m.Nombre, m.CampoPadre, m.Imagen
		Select Menu_Osp
		Set Order To Padre
		Seek m.Padre
		Do While !Eof("Menu_Osp") And Padre = m.Padre
			m.Registro = Recno("Menu_Osp")
			ThisForm.SelCar.Nodes.Add(m.Padre, 4, Clave, Prompt, 2, 3)
			ThisForm.SelCar.Nodes(Clave).Expanded = .F.
			ThisForm.Expande(Clave)
			Go m.Registro In Menu_Osp
			Skip 1 In Menu_Osp
		Enddo
		
	ENDPROC

