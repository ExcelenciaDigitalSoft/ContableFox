	PROCEDURE Command3.Click
		If Messagebox("¿Desea eliminar el nodo?",36,"¡ ATENCION !")=6
			Select Menu_Osp
			Set Order To Clave
			Seek ThisForm.SelCar.SelectedItem.Key
			If Found()
				_Clave=Clave
				If Left(Clave,1)="B"
					Delete
					&&Hijo
					ThisForm.Carga()
				Else
					If Right(Clave,2)="00"
						&&Abuelo
						If Messagebox("Va a eliminar todos sus hijos y nietos.Perdera toda la información"+Chr(13)+"¿Esta seguro de eliminar?",20,"¡ Precaución !")=6
							Delete For Substr(Clave,1,3)=Substr(_Clave,1,3)
							ThisForm.Carga()
						Endif
					Else
						&&Padre
						If Messagebox("Va a eliminar todos sus hijos"+Chr(13)+"¿Esta seguro de eliminar?",20,"¡ Precaución !")=6
							Delete For Padre=_Clave Or Clave=_Clave
							ThisForm.Carga()
						Endif
					Endif
				Endif
			Endif
		Endif
		
	ENDPROC

	PROCEDURE Command4.Click
		ThisForm.Nuevo=.T.
		Select Menu_Osp
		Set Order To Menu
		Go Bott
		_ultmasUno=Val(Substr(Clave,2,2))+1
		_ultPadre=Alltrim(Str(_ultmasUno))
		_ultPadre=Padr(_ultPadre,5,'0')
		_ultPadre="A"+_ultPadre
		Append Blank
		Replace Clave With _ultPadre
		ThisForm.txtPrompt.SetFocus
		ThisForm.Refresh
	ENDPROC

	PROCEDURE Command7.Click
		Select Menu_Osp
		Set Order To Clave
		Seek ThisForm.SelCar.SelectedItem.Key
		If Found()
			If Left(Clave,1)="B"
				=Messagebox("No se puede agregar hijo a un hijo",48,"Control")
				Return
			Endif
			ThisForm.Nuevo=.T.
			_Rec=Recno()
			_Clave=Clave
			If Right(_Clave,2)=="00"
				&& Agrega un hijo al abuelo
				Set Filter To Substr(Clave,1,3)=Substr(_Clave,1,3)
				Go Bott
				_NewClave="A"+Ceros(Str(Val(Right(Clave,5))+1,5),5)
				Append Blank
				_Rec=Recno()
				Replace Padre With _Clave,Clave With _NewClave
				*ThisForm.Carga()
			Else
				&& Agrega un hijo al padre
				Set Filter To Padre=_Clave
				Go Bottom
				_NewClave="B"+Ceros(Str(Val(Right(Clave,5))+1,5),5)
				Append Blank
				Replace Padre With _Clave,Clave With _NewClave
				_Rec=Recno()
				Set Filter To Left(_NewClave,3)=Left(Clave,3)
				*Replace All Clave With "B"+Ceros(Str(Val(Right(Clave,5))+1,5),5) For Right(Clave,5)=Right(_NewClave,5) And Val(Right(Clave,5))>=Val(Right(_newClave,5)) And Padre<>_Clave
				Replace All Clave With "B"+Ceros(Str(Val(Right(Clave,5))+1,5),5) For Val(Right(Clave,5))>=Val(Right(_newClave,5)) And Padre<>_Clave
				*Browse
			Endif
			Set Filter To 
			Go Record _Rec
			ThisForm.txtPrompt.SetFocus
		Endif
		ThisForm.Refresh
	ENDPROC

