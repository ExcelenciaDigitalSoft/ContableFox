	PROCEDURE ver_nivel
		Select Aux_niveles
		=TableRevert(.T.)
		Select Niveles
		Set Order To Operador
		Seek ThisForm.TxtCodigo.Value
		Do While Not Eof() And Nivel=ThisForm.TxtCodigo.Value
			Scatter Memvar
			m.Registro=Recno()
			Select Aux_Niveles
			Append Blank
			Gather Memvar
			Select Niveles
			Skip
		EndDo
		Select Aux_Niveles
		Set Order To CLAVE   && PADRE+CLAVE
		ThisForm.List1.Refresh
		ThisForm.List1.SetFocus
	ENDPROC

	PROCEDURE cmd1.Click
		Select Aux_MenuOsp
		=TableRevert(.T.)
		svalor=ThisForm.SelCar.SelectedItem.Index
		Select Menu_Osp
		Set Order To Clave
		Seek ThisForm.SelCar.SelectedItem.Key
		If Found()
			_Regis=Recno()
			_Clave=Clave
			_Padre=Padre
			_NewClave=""
			If Right(Clave,2)="00"
				&&Abuelo
				_NewClave=Left(_Clave,1)+Ceros(Str(Val(Substr(_Clave,2,2))-1,2),2)+Right(_Clave,3)
			Else
				&&Padre e Hijo
				_NewClave=Left(_Clave,3)+Ceros(Str(Val(Right(_Clave,3))-1,3),3)
			Endif
			Scatter Memvar
			m.Clave=_NewClave
			Select Aux_MenuOsp
			Append Blank
			Gather Memvar
			Select Menu_Osp
			Set Filter To Padre=_Padre
			Skip -1
			Scatter Memvar
			Skip
			m.Clave=_Clave
			Gather Memvar Fields Except IdPad
			Skip -1
			Select Aux_MenuOsp
			Scatter Memvar
			Select Menu_Osp
			Gather Memvar Fields Except IdPad
			Set Filter To 
			Select Aux_MenuOsp
			=Tablerevert(.T.)
			Select Menu_Osp
			If Left(Clave,1)="A"
				&&NOOOOO hijos
				Scan For Padre=_NewClave
					Scatter Memvar
					m.Registro=Recno()
					Select Aux_MenuOsp
					Append Blank
					Gather Memvar
				Endscan
				Replace All Padre With _NewClave For Padre=_Clave
				Select Aux_MenuOsp
				Scan
					Scatter Memvar
					Select Menu_Osp
					Go Record m.Registro
					m.Padre=_Clave
					Gather Memvar Fields Except IdPad
				Endscan
			Endif
			ThisForm.Carga()
			ThisForm.SelCar.Nodes.Item(svalor)
		Endif
		ThisForm.Refresh
	ENDPROC

