	PROCEDURE Xpbutton4.Click
		
		*if (Val(thisform.txtcodigo.value)=0)
		*	Messagebox("Debe Ingresar Un Proveedor")
		*	thisform.txtcodigo.setfocus
		*	return
		*Endif
		If Len(Alltrim(thisform.txtnombre.value))<4
			MessageBox("Debe Ingresar nombre del Proveedor o de la persona que entrega la Nota",48,"Control")
			Thisform.txtnombre.SetFocus
			Return
		EndIf
		if  Len(Alltr(Thisform.Text3.Value))=0
			Messagebox("Debe Ingresar Una Factura")
			*thisform.Text3.value = ''
			thisform.Text3.setfocus
			return
		Endif
		if (Val(thisform.Text4.value)=0)
			Messagebox("Debe Ingresar Una Factura")
			*thisform.Text4.value = ''
			thisform.Text4.setfocus
			return
		Endif
		if (Val(thisform.txtperiodo.value)=0)
			Messagebox("Debe Ingresar Un Periodo")
			return
		Endif
		if (thisform.txtFecha.value={})
			Messagebox("Debe Ingresar La Fecha")
			return
		Endif
		if len(alltrim(thisform.detalle.value)) =0
			messagebox("Debe ingresar un detalle")
			thisform.detalle.setfocus
			return
		endif 
		if messagebox("Confirma Grabar Factura",36,"Grabar")=6
			select aux_recepcionfac
			append blank
		 		replace numero with alltrim(thisform.Text2.value)+alltrim(thisform.Text3.value)+alltrim(thisform.Text4.value)
		 		replace proveedor with thisform.txtcodigo.value
			 	replace nomprov with thisform.txtnombre.value
		 		replace fecha with thisform.txtFecha.value
		 		replace detalle with thisform.detalle.value
		 		replace autoriza with thisform.Text5.value
		 		replace operador with (thisform.combo1.List(thisform.combo1.ListItemId))
		 		replace periodo with thisform.txtperiodo.value
		 		Replace cant with thisform.cantidadfac
		 		thisform.grdAux_recepcionfac.refresh
		   	    *Limpio pantalla
		   	    thisform.Text3.value = ''
		   	    thisform.Text4.value = ''
		   	    thisform.txtcodigo.value = ''
		   	    thisform.txtnombre.value = ''
		   	    thisform.detalle.value = ''
		   	    thisform.Text1.value = ''
		     	thisform.Text5.value = ''
		   	    *Invisible loguin
		   	    thisform.Label5.visible = .F.
			    thisform.Label6.visible = .F.
			    thisform.Text5.visible = .F.
			    thisform.Text1.visible = .F.
		   	    
		   	    thisform.txtcodigo.setfocus
		   	    	
		Endif
		 		
	ENDPROC

