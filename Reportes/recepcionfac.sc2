	PROCEDURE Init
		thisform.cartel=""
		select aux_recepcionfac_ver
		=tablerevert(.t.)
		thisform.txtFecha.value = date()
		thisform.txtPeriodo.Value=ceros(Str(Year(Date()),4),4)+ceros(Str(Month(Date()),2),2)
		
	ENDPROC

	PROCEDURE Command1.Click
		if messagebox("Confirma Marcar Todas Las Facturas",36,"Grabar")=6
			select aux_recepcionfac
			replace all ok with 1
			thisform.grdAux_recepcionfac.refresh()
		endif
	ENDPROC

	PROCEDURE Command2.Click
		if messagebox("Confirma Desmarcar Todas Las Facturas",36,"Grabar")=6
			select aux_recepcionfac
			replace all ok with 0
			thisform.grdAux_recepcionfac.refresh()
		endif
	ENDPROC

	PROCEDURE grdAux_recepcionfac_ver.DblClick
		thisform.grdAux_recepcionfac_ver.visible = .F.
		thisform.detalle.setfocus
		Messagebox("LA CARGA DE LA FACTURA REQUIERE AUTORIZACION")
	ENDPROC

	PROCEDURE Text1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		
	ENDPROC

	PROCEDURE Text1.LostFocus
		if val(this.value)#0
			select operadores
			set order to codigo
			seek alltr(thisform.text5.value)
			if found()
				if revisa = 0
					messagebox("USTED NO ESTA HABILITADO PARA AUTORIZAR FACTURAS")
					RETURN
				endif
				if alltr(coder10(contraseña))==alltr(thisform.text1.value) and autoriza = 1
					thisform.Xpbutton4.visible = .T.
					thisform.Xpbutton4.setfocus
					thisform.Label7.visible = .F.
				else
					=messagebox('Contraseña Incorrecta.',48,'Seguridad')	
					this.value=space(10)
					
					thisform.text1.value=space(3)
					thisform.text1.setfocus
				endif
			else
				if thisform.text1.value#'999'
					=messagebox('Codigo de Operador no Existente.',48,'Seguridad')	
					
					node
				endif	
			endif
		else
			if lastkey()#27
				if lastkey()=13 or 	lastkey()=24 or lastkey()=9 or lastkey()=25
					=messagebox('Debe Cargar su Contraseña.',48,'Seguridad')	
					node
				endif
			endif	
		endif
	ENDPROC

	PROCEDURE Text2.LostFocus
		Select aux_recepcionfac
		If Alltrim(this.Value)='NT'
			Set Order To nota
			Go bott
			thisform.text3.Value = "0000"
			thisform.text4.Value = ceros(Str(Val(Right(numero,8))+1,8),8)
			thisform.text3.Enabled=.f.
			thisform.text4.Enabled=.f.
		Else
			Set Order To 0	
			thisform.text3.Enabled=.t.
			thisform.text4.Enabled=.t.	
		EndIf
	ENDPROC

	PROCEDURE Text3.LostFocus
		this.Value=ceros(this.Value,4)
	ENDPROC

	PROCEDURE Text4.LostFocus
		this.Value=ceros(this.Value,8)
	ENDPROC

	PROCEDURE Text5.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		
	ENDPROC

	PROCEDURE Text5.LostFocus
		if val(this.value)#0
			this.value=ceros(this.value,3)
			if this.value#'999'
				select operadores
				set order to codigo
				seek this.value
				if found()
					if autoriza = 0
						messagebox("USTED NO ESTA HABILITADO PARA AUTORIZAR FACTURAS")
						this.value = ''
						thisform.text5.setfocus
					RETURN
				endif
				else
					=messagebox('Codigo de Operador no Existente.',48,'Seguridad')	
					thisform.text5.value=''
					thisform.text5.setfocus
					node
				endif
			else
				
			endif	
		else
			if lastkey()#27
				if lastkey()=13 or 	lastkey()=24 or lastkey()=9 or lastkey()=25
					=messagebox('Debe Cargar Codigo de Operador.',48,'Seguridad')
					thisform.text5.setfocus	
					node
				endif
			endif	
		endif
	ENDPROC

