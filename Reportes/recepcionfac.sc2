	PROCEDURE Xpbutton2.Click
		if messagebox("Confirma Grabar Factura",36,"Grabar")=6
			select aux_recepcionfac
			scan
			    if aux_recepcionfac.ok = 1
					select recepcionfac
					append blank
			 		replace numero with aux_recepcionfac.numero
			 		replace proveedor with aux_recepcionfac.proveedor
				 	replace nomprov with aux_recepcionfac.nomprov
			 		replace fecha with aux_recepcionfac.fecha
			 		replace detalle with aux_recepcionfac.detalle
			 		replace operador with aux_recepcionfac.operador
			 		replace periodo with aux_recepcionfac.periodo
			 		replace autoriza with aux_recepcionfac.autoriza
		 		Endif
			endscan
			Delete from aux_recepcionfac where aux_recepcionfac.ok = 1
			thisform.grdAux_recepcionfac.refresh
			Messagebox("La operacion se realizo con éxito")
		endif
	ENDPROC

