	PROCEDURE txtPeriodo.LostFocus
		thisform.cantidadfac=0
		select aux_recepcionfac_ver
		=tablerevert(.t.)
		select * from recepcionfac where proveedor = Alltrim(thisform.txtcodigo.value) and periodo = Alltrim(thisform.txtperiodo.value) into cursor control_fac
		select control_fac
		if reccount()>0
		 select control_fac
		 scan
		    
			scatter memvar
			select aux_recepcionfac_ver
			append blank
			gather memvar
		 	
		 EndScan
		 Endif
		 
		 select * from aux_recepcionfac where proveedor = Alltrim(thisform.txtcodigo.value) and periodo = Alltrim(thisform.txtperiodo.value) into cursor control_fac2
		 select control_fac2
		 if reccount()>0
			 scan
			 	scatter memvar
				select aux_recepcionfac_ver
				append blank
				gather memvar
			 endscan	
		 endif
		 Select aux_recepcionfac_ver
		 thisform.cantidadfac = Reccount()+1
